entation>															</xs:annotation>														</xs:element>														<xs:element ref="TPA_Extensions" minOccurs="0"/>													</xs:sequence>													<xs:attribute name="Age" type="Numeric0to999" use="optional">														<xs:annotation>															<xs:documentation xml:lang="en">This is used to specify a participant age in years.</xs:documentation>														</xs:annotation>													</xs:attribute>												</xs:complexType>											</xs:element>											<xs:element name="Group" minOccurs="0" maxOccurs="unbounded">												<xs:annotation>													<xs:documentation xml:lang="en">Group pricing.</xs:documentation>												</xs:annotation>												<xs:complexType>													<xs:sequence>														<xs:element name="Price" type="TourActivityChargeType" minOccurs="0">															<xs:annotation>																<xs:documentation xml:lang="en">Pricing for the specified group information.</xs:documentation>															</xs:annotation>														</xs:element>													</xs:sequence>													<xs:attribute name="GroupCode" type="xs:string" use="optional">														<xs:annotation>															<xs:documentation xml:lang="en">A group code that participant(s) are associated with.</xs:documentation>														</xs:annotation>													</xs:attribute>													<xs:attribute name="GroupName" type="xs:string" use="optional">														<xs:annotation>															<xs:documentation xml:lang="en">A group name that participant(s) are associated with.</xs:documentation>														</xs:annotation>													</xs:attribute>													<xs:attribute name="MinGroupSize" type="xs:positiveInteger" use="optional">														<xs:annotation>															<xs:documentation xml:lang="en">The minimum quantity of participants in a group.</xs:documentation>														</xs:annotation>													</xs:attribute>													<xs:attribute name="MaxGroupSize" type="