		<xs:annotation>
			<xs:documentation xml:lang="en">Base price and taxes associated with the tour and/or activity.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="TaxAmounts" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">A collection of one or more taxes associated with a specific charge.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="TaxAmount" maxOccurs="99">
							<xs:annotation>
								<xs:documentation xml:lang="en">Detailed information on one specific tax associated with a specific charge</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="Total" type="Money" use="required">
									<xs:annotation>
										<xs:documentation xml:lang="en">The total amount of the tax.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="TaxCode" type="StringLength1to16" use="optional">
									<xs:annotation>
										<xs:documentation xml:lang="en">A code for the tax that is agreed upon between trading partners.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="Percentage" type="Percentage" use="optional">
									<xs:annotation>
										<xs:documentation xml:lang="en">A percentage of the defined charge amount that this tax represents.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="Description" type="StringLength1to64" use="optional">
									<xs:annotation>
										<xs:documentation xml:lang="en">A description of the tax.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attributeGroup ref="CurrencyAmountGroup"/>
		<xs:attribute name="TaxInclusive" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">If TRUE, taxes are included in this charge.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Description" type="StringLength1to64" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">A description of the charge.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="GuaranteedInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">If true, a guarantee payment or hold is required.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="RateConvertInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">When true, the rate has been converted to a different currency.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MaxCharge" type="Money" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">The maximum amount that will be charged.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MinCharge" type="Money" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">The minimum amount that will be charged.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="TourActivityDescriptionType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Descriptive information that includes a short description, long description, URLs and multimedia.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ShortDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">A short description up to 500 characters.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="500"/>
						<xs:minLength value="1"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="LongDescription" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">A long description.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string"/>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Multimedia" type="MultimediaDescriptionsType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Multimedia information.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="URL" type="URL_Type" minOccurs="0" maxOccurs="5">
				<xs:annotation>
					<xs:documentation xml:lang="en">URLs to descriptive information.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="GuideInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">If true, guides are available.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="GuideOverview" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">A brief description of guide services.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="FreeChildrenQty" type="Numeric0to99" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">The number of free child places.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="TourActivityExtraRule">
		<xs:annotation>
			<xs:documentation xml:lang="en">Rules that apply to an extra.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ParticipantQualifier" minOccurs="0" maxOccurs="9">
				<xs:annotation>
					<xs:documentation xml:lang="en">Participant qualification information, including minimum age, actual age and/or age category, associated with this extra rule.

Qualifiers are used in combination with the @MandatoryInd and @OptionalInd to further qualify if an extra is optional or mandatory based on a customers age, birthdate and/or age category.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="AgeQualifier" type="AgeQualifierType" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="en">A specific type of age qualifier, such as "Adult". Select a value from the pre-defined list or select "Other_" and enter a value known between trading partners in the @Extension attribute.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="MinimumAge" type="xs:positiveInteger" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">The minimum age qualification for an extra.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="MaximumAge" type="xs:positiveInteger" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">The maximum age qualification for an extra.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="YearsExperience" type="xs:positiveInteger" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">The minimum number of years of experience required to book this extra e.g. driving experiience for car rental.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Age" type="Numeric0to999" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">This is used to specify a customers age in years.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attributeGroup ref="BirthDateGroup">
						<xs:annotation>
							<xs:documentation xml:lang="en">A customers birth date.</xs:documentation>
						</xs:annotation>
					</xs:attributeGroup>
				</xs:complexType>
			</xs:element>
			<xs:element name="OtherCriteria" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Other rule information associated with this extra, including payment form types accepted for deposits.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="DepositPaymentTypes" type="AcceptedPaymentsType" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="en">Accepted deposit payment types.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="MaxOccupancy" type="Numeric1to99" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">The maximum number of participants permitted for this extra (e.g. golf cart occupancy).</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="DateTimeReqInd" type="xs:boolean" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">If true, a start and end date/time must be associated with the extra request.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="StockControlledInd" type="xs:boolean" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">If true, the availabilty of this extra needs to be checked.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="DurationReqInd" type="xs:boolean" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">If true, a time duration must be supplied for the extra request.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="DepositInd" type="xs:boolean" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">If true, a deposit is required to secure the extra.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="MandatoryInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">If true, this extra is mandatory and must be included with the tour and/or activity.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="OptionalInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">If true, this extra is optional and may be included with and/or removed from the tour and/or activity.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ApplyTo" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Indicates whether the extra applies to all participants or only the participants that meet the specified rule criteria.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="All_Participants"/>
					<xs:enumeration value="Some_Participants"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Description" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">A description of the rule.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="TourActivityID_Type">
		<xs:annotation>
			<xs:documentation xml:lang="en">Unique identification for the tour/activity, including name, supplier ID and trading partner ID.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="TPA_Extensions" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="SupplierProductCode" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">The supplier/ operator unique code that identifies the tour and/or activity.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PartnerProductCode" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">A trading partner code that is associated with the supplier/operator product code.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SupplierBrandCode" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">The code for the supplier/ operator product brand.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Name" type="StringLength1to64" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">The name of the tour and/or activity.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ShortName" type="StringLength1to64" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">An abbreviated name for the tour and/or activity.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TourActivityID" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">A unique ID that identifies this tour and/or activity that may be referenced elsewhere in this message.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="TourActivityLanguageType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Languages spoken by the supplier/operator staff.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Code" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">The ISO 639 2 character language code.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="TourActivityLocationType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Location information for the tour and/or activity.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Address" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Identifies the physical address of the tour departure and/or activity location.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="AddressInfoType"/>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Region" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Describes regional information, which may include an island.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attributeGroup ref="RegionGroup">
						<xs:annotation>
							<xs:documentation xml:lang="en">Other region-based location type(s) that are agreed upon between trading partners, such as the name of an island.</xs:documentation>
						</xs:annotation>
					</xs:attributeGroup>
				</xs:complexType>
			</xs:element>
			<xs:element name="Position" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Describes geocoding information.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attributeGroup ref="PositionGroup"/>
					<xs:attribute name="Placemark" type="StringLength1to255" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">A free-form definition of the destination's position. Could be used as an online mapping service locator.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="TimeZone" type="StringLength1to64" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">The local time zone in which the tour/activity departs and or starts.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="TourActivityLodgingType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Participant lodging information if required by a tour supplier/ operator.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="HotelName" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">The name of the hotel and/or other lodging facility the participant and/or group is staying at.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="HotelLocation" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">The location of the hotel and/or other lodging facility.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="RoomNumber" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">The room number in the hotel and/or other lodging facility.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="TourActivityNegotiatedPricing">
		<xs:annotation>
			<xs:documentation xml:lang="en">Negotiated pricing for corporations and groups.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="AccountID" type="StringLength1to32" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">The customer account ID if applicable.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AccountName" type="StringLength1to128" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">The customer account name if applicable.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="RateCategory" type="OTA_CodeType" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">The rate category associated with the tour/ activity. Refer to OpenTravel Rate Category (RTC) code list.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="RateQualifier" type="StringLength1to32" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">This is the vendor specific code for rate codes (e.g. WES, 2A, DLY00).</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CorpDiscountNmbr" type="StringLength1to32" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">The vendor specific code used to identify a special rate associated with a specific organization.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="TourActivityParticipantCategoryType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Participant category information, which may be age, birth date and/or an age qualifier.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="QualifierInfo" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">A specific type of age and/or group qualifier, such as "Adult". Select a value from the pre-defined list or select "Other_" and enter a value known between trading partners in the @Extension attribute.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="AgeQualifierType">
							<xs:attribute name="ParticipantCategoryID" type="RPH_Type" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="en">A unique identifier for a participant category.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element ref="TPA_Extensions" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="Age" type="Numeric0to999" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">This is used to specify a participant age in years.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="BirthDate" type="xs:date" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Indicates the date of birth as indicated in the document, in ISO 8601 prescribed format.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Quantity" type="xs:nonNegativeInteger" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Used to define the quantity of participants for the specified information.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="TourActivityParticipantType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Participant information, which may be age, birth date and/or an age qualifier and name information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="CategoryInfo" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">A specific type of age and/or group qualifier, such as "Adult". Select a value from the pre-defined list or select "Other_" and enter a value known between trading partners in the @Extension attribute.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="AgeQualifierType">
							<xs:attribute name="ParticipantCategoryID" type="RPH_Type" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="en">A unique identifier for a participant category.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="ParticipantNameInfo" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Name information for participants.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="PersonNameType">
							<xs:sequence>
								<xs:element name="SpecialNeed" minOccurs="0" maxOccurs="5">
									<xs:annotation>
										<xs:documentation xml:lang="en">The name and/or code for a special need requirement, such as a wheelchair or vegetarian meal.</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:simpleContent>
											<xs:extension base="xs:string">
												<xs:attribute name="Code" type="xs:string" use="optional">
													<xs:annotation>
														<xs:documentation xml:lang="en">A code known between trading partners for a special needs item.</xs:documentation>
													</xs:annotation>
												</xs:attribute>
											</xs:extension>
										</xs:simpleContent>
									</xs:complexType>
								</xs:element>
								<xs:element name="LodgingInfo" type="TourActivityLodgingType" minOccurs="0">
									<xs:annotation>
										<xs:documentation xml:lang="en">Lodging information for the participant.</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
							<xs:attribute name="ParticipantID" type="RPH_Type" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="en">A unique identifier for a known participant that may be referenced elsewhere in this message.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="Gender" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="en">Identifies the gender.</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="Male"/>
										<xs:enumeration value="Female"/>
										<xs:enumeration value="Unknown"/>
										<xs:enumeration value="Male_NoShare"/>
										<xs:enumeration value="Female_NoShare"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
							<xs:attribute name="Nationality" type="ISO3166" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="en">The nationality of a participant, if known. Uses ISO 3166 Country Codes.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="LeadCustomerInd" type="xs:boolean" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="en">When 'true', indicates that this is the 'lead' participantr (i.e., the primary contact making the booking).</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element ref="TPA_Extensions" minOccurs="0"/>
		</xs:sequence>
		<xs:attribute name="Age" type="Numeric0to999" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">This is used to specify a participant age in years.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="BirthDate" type="xs:date" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Indicates the date of birth as indicated in the document, in ISO 8601 prescribed format.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Quantity" type="xs:nonNegativeInteger" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">Used to define the quantity of participants for the specified information.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="TourActivityPolicyType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Contains policies associated with the tour/activity.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Participant" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Participant quantity and age requirements.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="MinParticipants" type="xs:nonNegativeInteger" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">The minimum number of participants for the tour/activity.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="MaxParticipants" type="xs:positiveInteger" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">The maximum number of participants for the tour/activity.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="MinAge" type="xs:nonNegativeInteger" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">The minimum permitted age for participants for the tour/activity.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="MaxAge" type="xs:nonNegativeInteger" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">The maximum permitted age for participants on the tour/activity.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="Cancel" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Cancellation policy information.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="CancelDeadline" type="DateOrTimeOrDateTimeType" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="en">A date and/or date/time that defines when cancellations must be received before a penalty may be incurred.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="BookingDeadline" type="DateOrTimeOrDateTimeType" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="en">A date and/or date/time that defines when a reservation must be received by.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Other" minOccurs="0" maxOccurs="9">
				<xs:annotation>
					<xs:documentation xml:lang="en">Other policy description and optional type.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="Type" type="xs:string" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="en">The type of the policy.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="InsuranceReqInd" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">An indication of insurance requirements.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="NotRequired"/>
					<xs:enumeration value="Required_MustBePurchasedFromSupplier"/>
					<xs:enumeration value="Required_MayBeProvidedByParticipant"/>
					<xs:enumeration value="Unknown"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="InsuranceOverview" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">A brief description of insurance requirements and/or options.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="WaiverInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">If true, one or more participants may be required to sign a waiver.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="WaiverOverview" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">A brief description of waiver requirements.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="TourActivityPromotionType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Promotion details, including coupons and other discount plans.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Promotion" minOccurs="0" maxOccurs="9">
				<xs:annotation>
					<xs:documentation xml:lang="en">One or more descriptions, codes and effective dates  for promotional pricing (up to 5 may be specified.)</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="Code" type="xs:string" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="en">A promotion code.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="StartDate" type="DateOrDateTimeType" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="en">The starting date for a promotion.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="EndDate" type="DateOrTimeOrDateTimeType" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="en">The ending date for a promotion.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="Disclaimer" type="xs:string" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="en">Disclaimer related to a promotion.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="DiscountProgram" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Discount program details.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Detail" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="en">Details about a discount program.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Provider" type="CompanyNameType" minOccurs="0">
										<xs:annotation>
											<xs:documentation xml:lang="en">The company name associated with the discount program.</xs:documentation>
										</xs:annotation>
									</xs:element>
								</xs:sequence>
								<xs:attribute name="ProgramID" type="RPH_Type" use="optional">
									<xs:annotation>
										<xs:documentation xml:lang="en">A unique ID for this program that may be referenced elsewhere in this message.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="Name" type="xs:string" use="optional">
									<xs:annotation>
										<xs:documentation xml:lang="en">The name of a discount program, e.g. "Day Pass" or "Gold Card".</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="Description" type="xs:string" use="optional">
									<xs:annotation>
										<xs:documentation xml:lang="en">A description of the discount program.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
						<xs:element name="ParticipationInfo" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="en">Participation information, including card number and valid dates.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="ValidFromDate" type="DateOrDateTimeType" use="optional">
									<xs:annotation>
										<xs:documentation xml:lang="en">The starting date for a participant or group discount program.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="ValidThroughDate" type="DateOrTimeOrDateTimeType" use="optional">
									<xs:annotation>
										<xs:documentation xml:lang="en">The ending date for a participant or group discount program.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="ProgramNumber" type="xs:string" use="optional">
									<xs:annotation>
										<xs:documentation xml:lang="en">The card or other number associated with the program. </xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TourActivitySupplierType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Supplier and/or operator information, including address and contact details.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Name" type="CompanyNameType" minOccurs="0">
