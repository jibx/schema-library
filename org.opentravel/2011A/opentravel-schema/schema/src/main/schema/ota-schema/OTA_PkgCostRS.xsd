<?xml version="1.0" encoding="UTF-8"?><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.opentravel.org/OTA/2003/05" targetNamespace="http://www.opentravel.org/OTA/2003/05" elementFormDefault="qualified" version="2.000" id="OTA2011A">  <xs:include schemaLocation="OTA_PkgCommonTypes.xsd"/>  <xs:annotation>    <xs:documentation xml:lang="en">All Schema files in the OpenTravel Alliance specification are made available according to the terms defined by the OpenTravel License Agreement at http://www.opentravel.org/Specifications/Default.aspx.</xs:documentation>  </xs:annotation>  <xs:element name="OTA_PkgCostRS">    <xs:annotation>      <xs:documentation xml:lang="en">The message returns a basic costing for a requested package.</xs:documentation>    </xs:annotation>    <xs:complexType>      <xs:choice>        <xs:annotation>          <xs:documentation xml:lang="en">May have either errors or warnings - must have one if there is no success.</xs:documentation>        </xs:annotation>        <xs:sequence>          <xs:element name="Success" type="SuccessType">            <xs:annotation>              <xs:documentation xml:lang="en">The presence of this element indicates that the receiving system has successfully processed the preceding request message.</xs:documentation>            </xs:annotation>          </xs:element>          <xs:element name="Warnings" type="WarningsType" minOccurs="0">            <xs:annotation>              <xs:documentation xml:lang="en">The presence of this element indicates that, while the preceding request message has been successfully processed, the requeat may not have been fully satisfied.</xs:documentation>            </xs:annotation>          </xs:element>          <xs:element name="Package">            <xs:annotation>              <xs:documentation xml:lang="en">Details of the selected package.</xs:documentation>            </xs:annotation>            <xs:complexType>              <xs:complexContent>                <xs:extension base="PackageResponseType">                  <xs:sequence>                    <xs:element name="CostSummary" type="PkgInvoiceDetail" minOccurs="0">                      <xs:annotation>                        <xs:documentation xml:lang="en">A summary of the basic costs for the package.</xs:documentation>                      </xs:annotation>                    </xs:element>                    <xs:element name="AcceptedPaymtForms" minOccurs="0">                      <xs:annotation>                        <xs:documentation xml:lang="en">Details of the forms of payment accepted for this package. This item may not be present when there is a credit agreement in place.</xs:documentation>                      </xs:annotation>                      <xs:complexType>                        <xs:sequence>                          <xs:element name="AcceptedPaymtForm" maxOccurs="9">                            <xs:annotation>                              <xs:documentation xml:lang="en">Provides details of acceptable types of payment (e.g. Credit or Debit Card) together with applicable fees, if any.</xs:documentation>                            </xs:annotation>                            <xs:complexType>                              <xs:simpleContent>                                <xs:extension base="MonetaryRuleType">                                  <xs:attribute name="IssuerCode" type="StringLength1to16" use="optional">                                    <xs:annotation>                                      <xs:documentation xml:lang="en">Code identifying the issuer of a Switch debit card.</xs:documentation>                                    </xs:annotation>                                  </xs:attribute>                                </xs:extension>                              </xs:simpleContent>                            </xs:complexType>                          </xs:element>                        </xs:sequence>                      </xs:complexType>                    </xs:element>                  </xs:sequence>                </xs:extension>              </xs:complexContent>            </xs:complexType>          </xs:element>          <xs:element name="AvailableExtras" minOccurs="0">            <xs:annotation>              <xs:documentation xml:lang="en">A collection of extras which are bookable with the selected holiday package.</xs:documentation>            </xs:annotation>            <xs:complexType>              <xs:sequence>                <xs:element name="AvailableExtra" type="ExtrasInfoType" maxOccurs="99">                  <xs:annotation>                    <xs:documentation xml:lang="en">Details of an extra which is bookable with the selected holiday package.</xs:documentation>                  </xs:annotation>                </xs:element>              </xs:sequence>            </xs:complexType>          </xs:element>          <xs:element ref="TPA_Extensions" minOccurs="0"/>        </xs:sequence>        <xs:element name="Errors" type="ErrorsType">          <xs:annotation>            <xs:documentation xml:lang="en">The presence of this element indicates that the receiving system has been unable to process preceding request message.</xs:documentation>          </xs:annotation>        </xs:element>      </xs:choice>      <xs:attributeGroup ref="OTA_PayloadStdAttributes"/>      <xs:attributeGroup ref="CompanyID_AttributesGroup"/>      <xs:attributeGroup ref="CurrencyCodeGroup"/>    </xs:complexType>  </xs:element></xs:schema>