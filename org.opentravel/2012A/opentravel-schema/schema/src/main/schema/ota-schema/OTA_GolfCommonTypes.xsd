						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="CoursePar" type="xs:positiveInteger" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">The par for the course.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="CourseSlope" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation source="Description" xml:lang="en">The slope of the golf course.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Number" type="xs:positiveInteger" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">The hole number.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Yardage" type="xs:positiveInteger" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">The yardage for the hole.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Handicap" type="xs:positiveInteger" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">The handicap for the hole.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Slope" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">The slope for the hole.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="TeePosition" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation source="Description" xml:lang="en">The tee position for the hole.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Par" type="xs:positiveInteger" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">The par for the hole.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element ref="TPA_Extensions" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="TeeTimeAvailType">
