<?xml version="1.0" encoding="UTF-8"?><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.opentravel.org/OTA/2003/05" targetNamespace="http://www.opentravel.org/OTA/2003/05" elementFormDefault="qualified" version="2.000" id="OTA2011A">	<xs:include schemaLocation="OTA_TourInformation.xsd"/>	<xs:annotation>		<xs:documentation xml:lang="en">All Schema files in the OpenTravel Alliance specification are made available according to the terms defined by the OpenTravel License Agreement at http://www.opentravel.org/Specifications/Default.aspx.</xs:documentation>	</xs:annotation>	<xs:element name="OTA_TourAvailRQ">		<xs:annotation>			<xs:documentation xml:lang="en">This message is a request for availability for a specific tour.</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence>				<xs:element name="POS" type="POS_Type" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="en">Point of sale object</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="Tour">					<xs:annotation>						<xs:documentation xml:lang="en">Information identifying the tour for which availability is being requested.</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:sequence>							<xs:element name="DateRange" type="DateTimeSpanType" minOccurs="0">								<xs:annotation>									<xs:documentation xml:lang="en">The Start date and Duration or End Date of the tour.</xs:documentation>								</xs:annotation>							</xs:element>						</xs:sequence>						<xs:attribute name="ShortName" type="StringLength1to64" use="optional">							<xs:annotation>								<xs:documentation xml:lang="en">A short description of a holiday package.</xs:documentation>							</xs:annotation>						</xs:attribute>						<xs:attribute name="ID" type="StringLength1to255" use="optional">							<xs:annotation>								<xs:documentation xml:lang="en">The unique identifier of a tour.</xs:documentation>							</xs:annotation>						</xs:attribute>					</xs:complexType>				</xs:element>				<xs:element name="CustomerCount" minOccurs="0" maxOccurs="6">					<xs:annotation>						<xs:documentation xml:lang="en">Specifies the number of customers and optionally the customer type.</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:attributeGroup ref="TravelerCountGroup">							<xs:annotation>								<xs:documentation xml:lang="en">The quantity and definition of a customer type.</xs:documentation>							</xs:annotation>						</xs:attributeGroup>					</xs:complexType>				</xs:element>				<xs:element ref="TPA_Extensions" minOccurs="0"/>			</xs:sequence>			<xs:attributeGroup ref="OTA_PayloadStdAttributes"/>		</xs:complexType>	</xs:element></xs:schema>