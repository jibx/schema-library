<?xml version="1.0" encoding="UTF-8"?><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:ns="http://www.opentravel.org/OTA/2003/05/common" elementFormDefault="qualified" attributeFormDefault="unqualified" version="4.000" id="OTA2014B">	<xs:annotation>		<xs:documentation source="Description" xml:lang="en">ALL SCHEMA FILES IN THE OPENTRAVEL ALLIANCE SPECIFICATION ARE MADE AVAILABLE ACCORDING TO THE TERMS DEFINED BY THE OPENTRAVEL LICENSE AGREEMENT AT http://www.opentravel.org/Specifications/Default.aspx</xs:documentation>	</xs:annotation>	<xs:include schemaLocation="OTA_HotelCommonTypes.xsd"/>	<xs:include schemaLocation="OTA_TourActivityCommonTypes.xsd"/>	<xs:include schemaLocation="OTA_RailCommonTypes.xsd"/>	<xs:include schemaLocation="OTA_GroundCommonTypes.xsd"/>	<xs:complexType name="AccountInfoType">		<xs:annotation>			<xs:documentation xml:lang="en">Loyalty specific member profile information.</xs:documentation>		</xs:annotation>		<xs:sequence>			<xs:element name="MemberInfo" type="ContactPersonType">				<xs:annotation>					<xs:documentation xml:lang="en">Member information including name, contact information and employee information.</xs:documentation>				</xs:annotation>			</xs:element>			<xs:element name="MemberPreferences" minOccurs="0">				<xs:annotation>					<xs:documentation xml:lang="en">Loyalty program preferences specified by the enrolling member.</xs:documentation>				</xs:annotation>				<xs:complexType>					<xs:sequence>						<xs:element name="AdditionalReward" minOccurs="0" maxOccurs="5">							<xs:annotation>								<xs:documentation xml:lang="en">Additional programs that are honored by the primary loyalty account.</xs:documentation>							</xs:annotation>							<xs:complexType>								<xs:sequence>									<xs:element name="CompanyName" type="CompanyNameType" minOccurs="0"/>									<xs:element name="Name" type="PersonNameType" minOccurs="0"/>								</xs:sequence>								<xs:attribute name="MemberID" type="StringLength1to32" use="optional"/>							</xs:complexType>						</xs:element>						<xs:element name="Offer" minOccurs="0" maxOccurs="5">							<xs:annotation>								<xs:documentation xml:lang="en">Source from which members can receive information.</xs:documentation>							</xs:annotation>							<xs:complexType>								<xs:sequence>									<xs:element name="Communication" minOccurs="0" maxOccurs="5">										<xs:annotation>											<xs:documentation xml:lang="en">Preferred method of offer communication.</xs:documentation>										</xs:annotation>										<xs:complexType>											<xs:attribute name="DistribType" type="OTA_CodeType" use="optional">												<xs:annotation>													<xs:documentation xml:lang="en">An enumerated list of method of communication. Refer to OpenTravel Code List Distribution Type (DTB).</xs:documentation>												</xs:annotation>											</xs:attribute>										</xs:complexType>									</xs:element>								</xs:sequence>								<xs:attribute name="Type" use="optional">									<xs:annotation>										<xs:documentation xml:lang="en">An enumerated list of offer sources.</xs:documentation>									</xs:annotation>									<xs:simpleType>										<xs:restriction base="xs:string">											<xs:enumeration value="Partner"/>											<xs:enumeration value="Loyalty"/>										</xs:restriction>									</xs:simpleType>								</xs:attribute>							</xs:complexType>						</xs:element>					</xs:sequence>					<xs:attribute name="Awareness" type="StringLength1to32" use="optional">						<xs:annotation>							<xs:documentation xml:lang="en">Indicates how person became aware of loyalty program.</xs:documentation>						</xs:annotation>					</xs:attribute>					<xs:attributeGroup ref="PromotionCodeGroup">						<xs:annotation>							<xs:documentation xml:lang="en">Loyalty enrollment promotion code.</xs:documentation>						</xs:annotation>					</xs:attributeGroup>					<xs:attribute name="AwardsPreference" use="optional">						<xs:annotation>							<xs:documentation xml:lang="en">Method by which awards are allocated.</xs:documentation>						</xs:annotation>						<xs:simpleType>							<xs:restriction base="xs:string">								<xs:enumeration value="Points"/>								<xs:enumeration value="Miles"/>							</xs:restriction>						</xs:simpleType>					</xs:attribute>				</xs:complexType>			</xs:element>			<xs:element name="SecurityInfo" minOccurs="0">				<xs:annotation>					<xs:documentation xml:lang="en">Information allowing member to securely access account.</xs:documentation>				</xs:annotation>				<xs:complexType>					<xs:sequence>						<xs:element name="PasswordHint" minOccurs="0" maxOccurs="2">							<xs:annotation>								<xs:documentation xml:lang="en">Alternate method to password for account access.</xs:documentation>							</xs:annotation>							<xs:complexType>								<xs:simpleContent>									<xs:extension base="xs:string">										<xs:attribute name="Hint" use="optional">											<xs:simpleType>												<xs:restriction base="xs:string">													<xs:enumeration value="Question"/>													<xs:enumeration value="Answer"/>												</xs:restriction>											</xs:simpleType>										</xs:attribute>									</xs:extension>								</xs:simpleContent>							</xs:complexType>						</xs:element>					</xs:sequence>					<xs:attribute name="Username" type="StringLength1to32"/>					<xs:attribute name="Password" type="StringLength1to32"/>				</xs:complexType>			</xs:element>			<xs:element name="SubAccountBalance" minOccurs="0" maxOccurs="99">				<xs:annotation>					<xs:documentation xml:lang="en">Used to specify a sub-account and its point balance associated with this loyalty account.</xs:documentation>				</xs:annotation>				<xs:complexType>					<xs:attribute name="Type" type="StringLength1to16" use="optional">						<xs:annotation>							<xs:documentation xml:lang="en">Describes the type of sub account (e.g. miles, points, vouchers, stays).</xs:documentation>						</xs:annotation>					</xs:attribute>					<xs:attribute name="Balance" type="xs:integer" use="optional">						<xs:annotation>							<xs:documentation xml:lang="en">The current balance for this sub account.</xs:documentation>						</xs:annotation>					</xs:attribute>				</xs:complexType>			</xs:element>		</xs:sequence>		<xs:attribute name="PointBalance" type="xs:nonNegativeInteger" use="optional">			<xs:annotation>				<xs:documentation xml:lang="en">The point balance for a loyalty account.</xs:documentation>			</xs:annotation>		</xs:attribute>		<xs:attribute name="EnrollmentType" use="optional">			<xs:annotation>				<xs:documentation xml:lang="en">Method in which enrollment occurs.</xs:documentation>			</xs:annotation>			<xs:simpleType>				<xs:restriction base="xs:string">					<xs:enumeration value="Full"/>					<xs:enumeration value="Partial"/>				</xs:restriction>			</xs:simpleType>		</xs:attribute>		<xs:attribute name="EnrollmentMethod" type="OTA_CodeType" use="optional">			<xs:annotation>				<xs:documentation xml:lang="en">Means by which the enrollment was initiated. Refer to OpenTravel Code List Enrollment Method (ENR).</xs:documentation>			</xs:annotation>		</xs:attribute>		<xs:attribute name="AccountStatus" type="OTA_CodeType" use="optional">			<xs:annotation>				<xs:documentation xml:lang="en">The account status. Refer to Open Travel Code List Profile Status (PST).</xs:documentation>			</xs:annotation>		</xs:attribute>		<xs:attribute name="LastUpdateDate" type="xs:date" use="optional">			<xs:annotation>				<xs:documentation xml:lang="en">The date the balance was last updated. For example, in the case of an airline loyalty program, the date of the last flight taken.</xs:documentation>			</xs:annotation>		</xs:attribute>		<xs:attribute name="LoungeStatus" type="StringLength0to32" use="optional">			<xs:annotation>				<xs:documentation xml:lang="en">The lounge status of this account.</xs:documentation>			</xs:annotation>		</xs:attribute>		<xs:attributeGroup ref="ShareAllGroup"/>	</xs:complexType>	<xs:complexType name="LoyaltyTravelInfoType">		<xs:annotation>			<xs:documentation xml:lang="en">A choice between the various types of travel information supported by the loyalty program schema.</xs:documentation>		</xs:annotation>		<xs:choice>			<xs:element name="HotelStayInfo">				<xs:annotation>					<xs:documentation xml:lang="en">Information about the customers hotel stay.</xs:documentation>				</xs:annotation>				<xs:complexType>					<xs:sequence>						<xs:element name="ReservationID" type="UniqueID_Type" minOccurs="0">							<xs:annotation>								<xs:documentation xml:lang="en">A reservation ID and supporting information.</xs:documentation>							</xs:annotation>						</xs:element>						<xs:element name="RoomStayInfo" minOccurs="0">							<xs:annotation>								<xs:documentation xml:lang="en">Information about a room stay.</xs:documentation>							</xs:annotation>							<xs:complexType>								<xs:sequence>									<xs:element name="RoomType" type="RoomTypeType" minOccurs="0"/>									<xs:element name="RatePlan" type="RatePlanType" minOccurs="0"/>								</xs:sequence>								<xs:attributeGroup ref="DateTimeSpanGroup"/>							</xs:complexType>						</xs:element>					</xs:sequence>					<xs:attributeGroup ref="HotelReferenceGroup"/>				</xs:complexType>			</xs:element>			<xs:element name="AirFlightInfo">				<xs:annotation>					<xs:documentation xml:lang="en">Information about the customers air travel.</xs:documentation>				</xs:annotation>				<xs:complexType>					<xs:sequence>						<xs:element name="FlightSegment" type="BookFlightSegmentType" maxOccurs="9"/>					</xs:sequence>				</xs:complexType>			</xs:element>			<xs:element name="CarRentalInfo">				<xs:annotation>					<xs:documentation xml:lang="en">Information about the customers car rental.</xs:documentation>				</xs:annotation>				<xs:complexType>					<xs:sequence>						<xs:element name="CarSegment">							<xs:complexType>								<xs:sequence>									<xs:element name="Vendor" type="CompanyNameType" minOccurs="0">										<xs:annotation>											<xs:documentation xml:lang="en">Car rental supplier information.</xs:documentation>										</xs:annotation>									</xs:element>									<xs:element name="VehRentalCore" type="VehicleRentalCoreType" minOccurs="0">										<xs:annotation>											<xs:documentation xml:lang="en">Information that is common, or core, to all requests and responses associated with the reservation of a vehicle.</xs:documentation>										</xs:annotation>									</xs:element>									<xs:element name="Vehicle" type="VehicleCoreType" minOccurs="0">										<xs:annotation>											<xs:documentation xml:lang="en">Information on one specific vehicle along with detailed information on the charges associated with this vehicle.</xs:documentation>										</xs:annotation>									</xs:element>								</xs:sequence>							</xs:complexType>						</xs:element>					</xs:sequence>				</xs:complexType>			</xs:element>			<xs:element name="TourActivityInfo">				<xs:annotation>					<xs:documentation xml:lang="en">Information about the customers tours and activities.</xs:documentation>				</xs:annotation>				<xs:complexType>					<xs:sequence>						<xs:element name="Schedule">							<xs:annotation>								<xs:documentation xml:lang="en">The departure or tour/activity start date/time.</xs:documentation>							</xs:annotation>							<xs:complexType>								<xs:attributeGroup ref="DateTimeSpanGroup">									<xs:annotation>										<xs:documentation xml:lang="en">The start date/time, end date/time and optional duration.</xs:documentation>									</xs:annotation>								</xs:attributeGroup>							</xs:complexType>						</xs:element>						<xs:element name="Discount" minOccurs="0" maxOccurs="unbounded">							<xs:annotation>								<xs:documentation xml:lang="en">Promotion code and discount program information that may be associated with an individual participant, a participant category and/or a group.</xs:documentation>							</xs:annotation>							<xs:complexType>								<xs:sequence>									<xs:element name="PromotionCode" type="xs:string" minOccurs="0" maxOccurs="9">										<xs:annotation>											<xs:documentation xml:lang="en">One or more promotion codes for promotional pricing.Up to 9 may be specified for each individual participant, participant category and/ or group.</xs:documentation>										</xs:annotation>									</xs:element>									<xs:element name="ParticipationInfo" minOccurs="0">										<xs:annotation>											<xs:documentation xml:lang="en">Participation information, including card number and valid dates.</xs:documentation>										</xs:annotation>										<xs:complexType>											<xs:attribute name="ValidFromDate" type="DateOrDateTimeType" use="optional">												<xs:annotation>													<xs:documentation xml:lang="en">The starting date for a participant or group discount program.</xs:documentation>												</xs:annotation>											</xs:attribute>											<xs:attribute name="ValidThroughDate" type="DateOrTimeOrDateTimeType" use="optional">												<xs:annotation>													<xs:documentation xml:lang="en">The ending date for a participant or group discount program.</xs:documentation>												</xs:annotation>											</xs:attribute>											<xs:attribute name="ProgramNumber" type="xs:string" use="optional">												<xs:annotation>													<xs:documentation xml:lang="en">The card or other number associated with the program. </xs:documentation>												</xs:annotation>											</xs:attribute>											<xs:attribute name="NameOnCard" type="xs:string" use="optional">												<xs:annotation>													<xs:documentation xml:lang="en">The name (individual or corporate) on the discount card.</xs:documentation>												</xs:annotation>											</xs:attribute>										</xs:complexType>									</xs:element>								</xs:sequence>								<xs:attributeGroup ref="TourActivityParticipantGroup">									<xs:annotation>										<xs:documentation xml:lang="en">Unique ID(s) assigned elsewhere in the message that associate discount program and promotion information with an individual participant, a participant category and/or a group.</xs:documentation>									</xs:annotation>								</xs:attributeGroup>							</xs:complexType>						</xs:element>						<xs:element name="Extra" minOccurs="0" maxOccurs="unbounded">							<xs:annotation>								<xs:documentation xml:lang="en">Extra amenities being requested and/or reserved.</xs:documentation>							</xs:annotation>							<xs:complexType>								<xs:sequence>									<xs:element name="Deposit" minOccurs="0">										<xs:annotation>											<xs:documentation xml:lang="en">Deposit payment information if a deposit is required for the extra.</xs:documentation>										</xs:annotation>										<xs:complexType>											<xs:complexContent>												<xs:extension base="PaymentDetailType"/>											</xs:complexContent>										</xs:complexType>									</xs:element>								</xs:sequence>								<xs:attribute name="SupplierCode" type="xs:string" use="optional">									<xs:annotation>										<xs:documentation xml:lang="en">The supplier code that uniquely identifies the extra.</xs:documentation>									</xs:annotation>								</xs:attribute>								<xs:attribute name="OtherCode" type="xs:string" use="optional">									<xs:annotation>										<xs:documentation xml:lang="en">The trading partner code that uniquely identifies the extra and is associated with the supplier code.</xs:documentation>									</xs:annotation>								</xs:attribute>								<xs:attribute name="Name" type="StringLength1to64" use="optional">									<xs:annotation>										<xs:documentation xml:lang="en">The extra amenity name.</xs:documentation>									</xs:annotation>								</xs:attribute>								<xs:attribute name="Description" type="xs:string" use="optional">									<xs:annotation>										<xs:documentation xml:lang="en">A description of the extra.</xs:documentation>									</xs:annotation>								</xs:attribute>								<xs:attribute name="AdditionalInfo" type="xs:string" use="optional">									<xs:annotation>										<xs:documentation xml:lang="en">Identifies additional information for an extra. E.g. "Large", "Medium" or "Small" frame for a mountain bike.</xs:documentation>									</xs:annotation>								</xs:attribute>								<xs:attribute name="Quantity" type="xs:positiveInteger" use="optional">									<xs:annotation>										<xs:documentation xml:lang="en">The quantity of extras being requested.</xs:documentation>									</xs:annotation>								</xs:attribute>								<xs:attribute name="ReserveInd" type="xs:boolean" use="optional">									<xs:annotation>										<xs:documentation xml:lang="en">If true, a reservation for this this extra is requested.</xs:documentation>									</xs:annotation>								</xs:attribute>								<xs:attributeGroup ref="TourActivityParticipantGroup">									<xs:annotation>										<xs:documentation xml:lang="en">Unique ID(s) assigned elsewhere in the message that associate extras information with an individual participant, a participant category and/or a group.</xs:documentation>									</xs:annotation>								</xs:attributeGroup>							</xs:complexType>						</xs:element>						<xs:element name="Location" type="TourActivityLocationType" minOccurs="0">							<xs:annotation>								<xs:documentation xml:lang="en">The tour/ activity location being booked.</xs:documentation>							</xs:annotation>						</xs:element>						<xs:element name="NegotiatedRate" type="TourActivityNegotiatedPricing" minOccurs="0" maxOccurs="9">							<xs:annotation>								<xs:documentation xml:lang="en">Negotiated rate information that may be associated with an individual participant, a participant category and/or a group.</xs:documentation>							</xs:annotation>						</xs:element>						<xs:element name="PickupDropoff" minOccurs="0" maxOccurs="2">							<xs:annotation>								<xs:documentation xml:lang="en">The pickup and/or dropoff information if transportation is provided to/ from the tour/activity location.</xs:documentation>							</xs:annotation>							<xs:complexType>								<xs:complexContent>									<xs:extension base="TourActivityTransRequestType">										<xs:sequence>											<xs:element name="ParticipantList" minOccurs="0" maxOccurs="unbounded">												<xs:annotation>													<xs:documentation xml:lang="en">References to individual participants, a participant category and/ or a group.</xs:documentation>												</xs:annotation>												<xs:complexType>													<xs:attributeGroup ref="TourActivityParticipantGroup">														<xs:annotation>															<xs:documentation xml:lang="en">Unique ID(s) assigned elsewhere in the message that associate pickup/ dropoff information with an individual participant, a participant category and/or a group.</xs:documentation>														</xs:annotation>													</xs:attributeGroup>												</xs:complexType>											</xs:element>										</xs:sequence>									</xs:extension>								</xs:complexContent>							</xs:complexType>						</xs:element>						<xs:element name="Pricing" minOccurs="0">							<xs:annotation>								<xs:documentation xml:lang="en">Pricing information for the tour/ activity that may be broken down by participant category and/or a group.</xs:documentation>							</xs:annotation>							<xs:complexType>								<xs:sequence>									<xs:element name="Summary" minOccurs="0">										<xs:annotation>											<xs:documentation xml:lang="en">Pricing information for the tour/ activity that is not assciated with participant category(s) and/or group(s).</xs:documentation>										</xs:annotation>										<xs:complexType>											<xs:complexContent>												<xs:extension base="TourActivityChargeType">													<xs:sequence>														<xs:element name="PricingType" minOccurs="0">															<xs:annotation>																<xs:documentation xml:lang="en">The pricing type, e.g. per person and per group. Select a predefined pricing type from the list or select the "Other_" value and enter a pricing type known to your trading partners in the @Extension field.</xs:documentation>															</xs:annotation>															<xs:complexType>																<xs:simpleContent>																	<xs:extension base="TourActivityPricingTypeEnum">																		<xs:attribute name="Extension" type="xs:string" use="optional">																			<xs:annotation>																				<xs:documentation xml:lang="en">A pricing type known to trading partners if the "Other_" value has been selected from the list.</xs:documentation>																			</xs:annotation>																		</xs:attribute>																	</xs:extension>																</xs:simpleContent>															</xs:complexType>														</xs:element>													</xs:sequence>												</xs:extension>											</xs:complexContent>										</xs:complexType>									</xs:element>									<xs:element name="ParticipantCategory" minOccurs="0" maxOccurs="unbounded">										<xs:annotation>											<xs:documentation xml:lang="en">Pricing for one or more participant category(s).</xs:documentation>										</xs:annotation>										<xs:complexType>											<xs:sequence>												<xs:element name="QualifierInfo" minOccurs="0">													<xs:annotation>														<xs:documentation xml:lang="en">A specific type of age and/or group qualifier, such as "Adult". Select a value from the pre-defined list or select "Other_" and enter a value known between trading partners in the @Extension attribute.</xs:documentation>													</xs:annotation>													<xs:complexType>														<xs:complexContent>															<xs:extension base="AgeQualifierType"/>														</xs:complexContent>													</xs:complexType>												</xs:element>												<xs:element name="Price" type="TourActivityChargeType" minOccurs="0">													<xs:annotation>														<xs:documentation xml:lang="en">Pricing for the specified participant category information.</xs:documentation>													</xs:annotation>												</xs:element>												<xs:element ref="TPA_Extensions" minOccurs="0"/>											</xs:sequence>											<xs:attribute name="Age" type="Numeric0to999" use="optional">												<xs:annotation>													<xs:documentation xml:lang="en">This is used to specify a participant age in years.</xs:documentation>												</xs:annotation>											</xs:attribute>										</xs:complexType>									</xs:element>									<xs:element name="Group" minOccurs="0" maxOccurs="unbounded">										<xs:annotation>											<xs:documentation xml:lang="en">Group pricing.</xs:documentation>										</xs:annotation>										<xs:complexType>											<xs:sequence>												<xs:element name="Price" type="TourActivityChargeType" minOccurs="0">													<xs:annotation>														<xs:documentation xml:lang="en">Pricing for the specified group information.</xs:documentation>													</xs:annotation>												</xs:element>											</xs:sequence>											<xs:attribute name="GroupCode" type="xs:string" use="optional">												<xs:annotation>													<xs:documentation xml:lang="en">A group code that participant(s) are associated with.</xs:documentation>												</xs:annotation>											</xs:attribute>											<xs:attribute name="GroupName" type="xs:string" use="optional">												<xs:annotation>													<xs:documentation xml:lang="en">A group name that participant(s) are associated with.</xs:documentation>												</xs:annotation>											</xs:attribute>											<xs:attribute name="MinGroupSize" type="xs:positiveInteger" use="optional">												<xs:annotation>													<xs:documentation xml:lang="en">The minimum quantity of participants in a group.</xs:documentation>												</xs:annotation>											</xs:attribute>											<xs:attribute name="MaxGroupSize" type="xs:positiveInteger" use="optional">												<xs:annotation>													<xs:documentation xml:lang="en">The maximum quantity of participants in a group.</xs:documentation>												</xs:annotation>											</xs:attribute>											<xs:attribute name="KnownGroupSize" type="xs:positiveInteger" use="optional">												<xs:annotation>													<xs:documentation xml:lang="en">The actual quantity of participants in a group.</xs:documentation>												</xs:annotation>											</xs:attribute>										</xs:complexType>									</xs:element>								</xs:sequence>							</xs:complexType>						</xs:element>						<xs:element name="SupplierOperator" type="TourActivitySupplierType" minOccurs="0" maxOccurs="2">							<xs:annotation>								<xs:documentation xml:lang="en">Information about the supplier/ operator of the tour/ activity.</xs:documentation>							</xs:annotation>						</xs:element>					</xs:sequence>				</xs:complexType>			</xs:element>			<xs:element name="GroundTransportationInfo">				<xs:annotation>					<xs:documentation xml:lang="en">Information about the customers ground transportation.</xs:documentation>				</xs:annotation>				<xs:complexType>					<xs:sequence>						<xs:element name="Locations" type="GroundLocationsType" minOccurs="0">							<xs:annotation>								<xs:documentation xml:lang="en">Pickup, interim stops and drop-off details.</xs:documentation>							</xs:annotation>						</xs:element>						<xs:element name="ServiceInfo" type="GroundServiceDetailType" minOccurs="0">							<xs:annotation>								<xs:documentation xml:lang="en">Passenger preferences and information, requested and/or required, for this ground transportation service, including service type, disability vehicle, fuel efficient vehicle and other vehicle preferences and services.</xs:documentation>							</xs:annotation>						</xs:element>					</xs:sequence>				</xs:complexType>			</xs:element>			<xs:element name="RailInfo">				<xs:annotation>					<xs:documentation xml:lang="en">Information about the customers rail travel.</xs:documentation>				</xs:annotation>				<xs:complexType>					<xs:sequence>						<xs:element name="OriginDestination" maxOccurs="9">							<xs:annotation>								<xs:documentation xml:lang="en">Details about one specific journey between this origin and destination location. This element will be repeated for each journey.  For example, for a round-trip reservation, this element will occur two times.</xs:documentation>							</xs:annotation>							<xs:complexType>								<xs:sequence>									<xs:element name="OriginLocation" type="LocationType" minOccurs="0">										<xs:annotation>											<xs:documentation xml:lang="en">The travel origin location.</xs:documentation>										</xs:annotation>									</xs:element>									<xs:element name="DestinationLocation" type="LocationType" minOccurs="0">										<xs:annotation>											<xs:documentation xml:lang="en">The travel destination location.</xs:documentation>										</xs:annotation>									</xs:element>									<xs:element name="TrainSegment" maxOccurs="9">										<xs:annotation>											<xs:documentation xml:lang="en">A collection of train segments that provide journey information from origin to destination. Each segment has full details on one specific train segment, including the origin and destination locations for this segment,related travelers' request, etc.</xs:documentation>										</xs:annotation>										<xs:complexType>											<xs:complexContent>												<xs:extension base="TrainSegmentSummaryType">													<xs:sequence>														<xs:element name="ClassCodes" type="ClassCodeType" minOccurs="0" maxOccurs="99">															<xs:annotation>																<xs:documentation xml:lang="en">The class of inventory controlled services available.</xs:documentation>															</xs:annotation>														</xs:element>														<xs:element name="TravelerRequest" minOccurs="0" maxOccurs="99">															<xs:annotation>																<xs:documentation xml:lang="en">Specific traveler preferences for the booking request, including accommodation selection and discount type.</xs:documentation>															</xs:annotation>															<xs:complexType>																<xs:sequence>																	<xs:element name="DiscountType" minOccurs="0">																		<xs:annotation>																			<xs:documentation xml:lang="en">A traveler requested discount type.</xs:documentation>																		</xs:annotation>																		<xs:complexType>																			<xs:attributeGroup ref="RailCodeGroup">																				<xs:annotation>																					<xs:documentation xml:lang="en">Used to specify a ticket class code and optionally the source for the code.Note that the Code may be a value from the OpenTravel Code List "Discounts Available" (DIS) or a string value up to 8 characters in length. </xs:documentation>																				</xs:annotation>																			</xs:attributeGroup>																		</xs:complexType>																	</xs:element>																	<xs:element ref="AccommodationService"/>																</xs:sequence>																<xs:attribute name="TravelerRPH" type="RPH_Type" use="optional">																	<xs:annotation>																		<xs:documentation xml:lang="en">A reference to the traveler or traveler category. Note that this is only used to relate data within this message.</xs:documentation>																	</xs:annotation>																</xs:attribute>															</xs:complexType>														</xs:element>														<xs:element name="MarketingCompany" type="CompanyNameType" minOccurs="0">															<xs:annotation>																<xs:documentation>The marketing train company associated with this segment.</xs:documentation>															</xs:annotation>														</xs:element>													</xs:sequence>													<xs:attribute name="SeatPreference" type="SeatType" use="optional">														<xs:annotation>															<xs:documentation xml:lang="en">Used to specify a seat preference, e.g. aisle, window, etc.</xs:documentation>														</xs:annotation>													</xs:attribute>													<xs:attribute name="SeatDirection" type="SeatDirectionType" use="optional">														<xs:annotation>															<xs:documentation xml:lang="en">The direction the seat faces.</xs:documentation>														</xs:annotation>													</xs:attribute>												</xs:extension>											</xs:complexContent>										</xs:complexType>									</xs:element>								</xs:sequence>							</xs:complexType>						</xs:element>						<xs:element name="RateQualifier" type="RailRateQualifyingType" minOccurs="0">							<xs:annotation>								<xs:documentation xml:lang="en">Indicates the type of fares of interest to the customer, along with any discount number or promotional codes that may affect the fare.</xs:documentation>							</xs:annotation>						</xs:element>						<xs:element name="Fulfillment" type="CompanyNameType" minOccurs="0">							<xs:annotation>								<xs:documentation xml:lang="en">Information on the fulfillment of the ticket, e.g. the party that will be issuing the ticket.</xs:documentation>							</xs:annotation>						</xs:element>					</xs:sequence>				</xs:complexType>			</xs:element>		</xs:choice>	</xs:complexType></xs:schema>