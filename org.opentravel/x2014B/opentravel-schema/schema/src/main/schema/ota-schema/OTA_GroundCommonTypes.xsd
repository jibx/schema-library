	<xs:complexType name="GroundServiceChargesType">
		<xs:annotation>
			<xs:documentation source="Description" xml:lang="en">Charge details related to a vehicle/ ground transportation service.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="ChargePurpose" type="List_VehChargePurpose" minOccurs="0" maxOccurs="1">
