<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:ns="http://www.opentravel.org/OTA/2003/05/common" elementFormDefault="qualified" version="2.000" id="OTA2014B">
	<xs:annotation>
		<xs:documentation source="Description" xml:lang="en">ALL SCHEMA FILES IN THE OPENTRAVEL ALLIANCE SPECIFICATION ARE MADE AVAILABLE ACCORDING TO THE TERMS DEFINED BY THE OPENTRAVEL LICENSE AGREEMENT AT http://www.opentravel.org/Specifications/Default.aspx</xs:documentation>
	</xs:annotation>
	<xs:include schemaLocation="OTA_CommonTypes.xsd"/>
	<xs:include schemaLocation="OTA_VehicleCommonTypes.xsd"/>
	<xs:complexType name="GroundAirportType">
		<xs:annotation>
			<xs:documentation source="Description" xml:lang="en">An airport definition for a ground service.</xs:documentation>
		</xs:annotation>
		<xs:attributeGroup ref="AirportLocationGroup"/>
		<xs:attribute name="AirportName" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">The name of the airport.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="GroundFeesType">
		<xs:annotation>
			<xs:documentation source="Description" xml:lang="en">Fees that are applied in addition to the Ground Transportation Service charges for this reservation. 

NOTE: Non-base charges, such as the basic price of the service are typically specified in the ServiceCharges element, while additional fees, such as gratuity, are specified here.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Fee" type="VehicleChargePurposeType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="GroundServiceType">
		<xs:annotation>
			<xs:documentation source="Description" xml:lang="en">Service and vehicle information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Language" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation source="Description" xml:lang="en">Language details pertaining to the service.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="Language" type="xs:language" use="optional">
						<xs:annotation>
							<xs:documentation source="Description" xml:lang="en">Language spoken by the driver or the guide.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="PrimaryLangInd" type="xs:boolean" use="optional">
						<xs:annotation>
							<xs:documentation source="Description" xml:lang="en">If true, this is the primary language spoken by the driver or the guide.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="RequestedInd" type="xs:boolean" use="optional">
						<xs:annotation>
							<xs:documentation source="Description" xml:lang="en">If true, it is preferred that the driver speak this language.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="ChildCarSeatInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">If true, a child car seat is required or requested.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PetFriendlyInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">If true, a vehicle is being requested and/or is available that allows domestic pets.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="FuelEfficientVehInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">If true, the vehicle is fuel efficient.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MaximumPassengers" type="xs:positiveInteger" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">The maximum quantity of passengers this vehicle can accommodate. NOTE: This figure is based on the quantity of seat belts in the vehicle.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MaximumBaggage" type="xs:positiveInteger" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">The (recommended) maximum baggage that can be stored in the vehicles trunk.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="GreetingSignInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">If true, a greeting sign is requested.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="GreetingSignName" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">The name to be on the greeting sign.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MeetAndGreetInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">If true, complementary meet and greet services are requested and/or provided..</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PersonalGreeterInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">If true, personal meet and greet is requested (that may incur an additional charge) or available.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="GuideInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">If true, guide services are requested and/or provided for local area knowledge and information.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ShortDescription">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">A short description of the service type and vehicle (up to 250 characters.)</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="0"/>
					<xs:maxLength value="250"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="Notes" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">Other notes related to the ground service, such as "Driver will meet you in baggage claim."</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MultilingualInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">If true, specific languages are requested and/or provided in the Language element.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="GroundServiceDetailType">
		<xs:annotation>
			<xs:documentation source="Description" xml:lang="en">Passenger preferences requested and/or required for a ground transportation service, including service type, disability vehicle and other vehicle preferences.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="GroundServiceType">
				<xs:sequence>
					<xs:element name="Location" type="GroundLocationsType" minOccurs="0">
						<xs:annotation>
							<xs:documentation source="Description" xml:lang="en">Pickup, stop and drop off location information.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ServiceLevel" type="List_LevelOfService" minOccurs="0">
						<xs:annotation>
							<xs:documentation source="Description" xml:lang="en">An extensible list of service levels.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="VehicleType" type="List_VehCategory" minOccurs="0">
						<xs:annotation>
							<xs:documentation source="Description" xml:lang="en">An extensible list of vehicle types and/or categories.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="VehicleSize" type="List_VehSize" minOccurs="0">
						<xs:annotation>
							<xs:documentation source="Description" xml:lang="en">An extensible list of vehicle sizes.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="DisabilityVehicleInd" type="xs:boolean" use="required">
					<xs:annotation>
						<xs:documentation source="Description" xml:lang="en">If true, this vehicle is disability compatible.</xs:documentation>
					</xs:annotation>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="GroundLocationType">
		<xs:annotation>
			<xs:documentation source="Description" xml:lang="en">Service location details.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Address" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="Description" xml:lang="en">A physical address or location type and name.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="AddressInfoType">
							<xs:sequence>
								<xs:element name="LocationType" type="List_GroundLocationType" minOccurs="1">
									<xs:annotation>
										<xs:documentation source="Description" xml:lang="en">The type of location.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="LocationName" type="StringLength1to128" minOccurs="0">
									<xs:annotation>
										<xs:documentation source="Description" xml:lang="en">The name of the location, which may be a hotel or airport name.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="SpecialInstructions" minOccurs="0">
									<xs:annotation>
										<xs:documentation source="Description" xml:lang="en">Special instructions regarding the pickup, stop or drop off.</xs:documentation>
									</xs:annotation>
									<xs:simpleType>
										<xs:restriction base="StringLength1to255"/>
									</xs:simpleType>
								</xs:element>
								<xs:element name="Directions" type="FormattedTextType" minOccurs="0">
									<xs:annotation>
										<xs:documentation source="Description" xml:lang="en">Textual directions regarding the pickup, stop or drop off.</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
							<xs:attributeGroup ref="PositionGroup"/>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="AirportInfo" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="Description" xml:lang="en">Airport details, including terminal and gate.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Arrival" type="GroundAirportType" minOccurs="0">
							<xs:annotation>
								<xs:documentation source="Description" xml:lang="en">Arrival airport information.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="Departure" type="GroundAirportType" minOccurs="0">
							<xs:annotation>
								<xs:documentation source="Description" xml:lang="en">Departure airport information.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Airline" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="Description" xml:lang="en">Airline details, including flight number.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="OperatingAirlineType">
							<xs:attribute name="FlightDateTime" type="DateOrDateTimeType" use="optional">
								<xs:annotation>
									<xs:documentation source="Description" xml:lang="en">The flight departure and/or arrival time and date.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="DateTime" type="DateOrDateTimeType" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">The date and time of the service.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="GroundLocationsType">
		<xs:annotation>
			<xs:documentation source="Description" xml:lang="en">Pickup, interim stops and drop-off details.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Pickup" type="GroundLocationType">
				<xs:annotation>
					<xs:documentation source="Description" xml:lang="en">Pickup details, including date and time, address, airport and airline information.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Stops" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="Description" xml:lang="en">A collection of Stop information, including address, airport and airline details.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Stop" maxOccurs="unbounded">
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="GroundLocationType">
										<xs:attribute name="Sequence" type="xs:positiveInteger" use="optional">
											<xs:annotation>
												<xs:documentation source="Description" xml:lang="en">A sequence number for the stop.</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="Dropoff" type="GroundLocationType">
				<xs:annotation>
					<xs:documentation source="Description" xml:lang="en">Drop off information, including address, airport and airline details.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="GroundPassengerInfoType">
		<xs:annotation>
			<xs:documentation source="Description" xml:lang="en">Passenger information that may determine the class of vehicle required.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="MaximumPassengers" type="xs:positiveInteger" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">The maximum quantity of passengers requiring ground service.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MaximumBaggage" type="xs:positiveInteger" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">The maximum baggage for all passengers.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ChildCarSeatInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">If true, a child car seat is required or requested.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="GroundPrimaryAdditionalPassengerType">
		<xs:annotation>
			<xs:documentation source="Description" xml:lang="en">Information on the one primary passenger and, optionally, several additional passengers.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Primary">
				<xs:annotation>
					<xs:documentation source="Description" xml:lang="en">Information on the primary passenger (who is the contact for this reservation.)</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="ContactPersonType">
							<xs:sequence>
								<xs:element name="CustomerID" type="UniqueID_Type" minOccurs="0">
									<xs:annotation>
										<xs:documentation source="Description" xml:lang="en">The identification of the customer for whom a booking is being requested. This is different from the customer's loyalty number.</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Additional" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation source="Description" xml:lang="en">Information on additional passengers. This may also be used to pass an event manifest, e.g. a list of passengers that will have vehicles allocated by the supplier.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="ContactPersonType">
							<xs:sequence>
								<xs:element name="AdditionalPersonType" minOccurs="0">
									<xs:annotation>
										<xs:documentation source="Description" xml:lang="en">Used to define the specific type of additional person.</xs:documentation>
									</xs:annotation>
									<xs:complexType>
										<xs:attributeGroup ref="TravelerCountGroup"/>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
							<xs:attributeGroup ref="DateTimeSpanGroup">
								<xs:annotation>
									<xs:documentation source="Description" xml:lang="en">Specifies the dates for how long the additional driver should be part of the rental contract.</xs:documentation>
								</xs:annotation>
							</xs:attributeGroup>
							<xs:attribute name="CorpDiscountName" type="StringLength1to64" use="optional">
								<xs:annotation>
									<xs:documentation source="Description" xml:lang="en">This is the name of the organization associated with the corporate discount number.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="CorpDiscountNmbr" type="StringLength1to32" use="optional">
								<xs:annotation>
									<xs:documentation source="Description" xml:lang="en">This is the code used to identify if the additional driver is eligible for benefits associated with a specific organization.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="QualificationMethod" use="optional">
								<xs:annotation>
									<xs:documentation source="Description" xml:lang="en">Used as a qualification for an additional driver.</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="RT_AirlineTicket">
											<xs:annotation>
												<xs:documentation source="Description" xml:lang="en">Round trip airline ticket</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="CreditCard">
											<xs:annotation>
												<xs:documentation source="Description" xml:lang="en">If needed, the credit card details should be passed in PaymentForm/PaymentCard.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="PassportAndReturnTkt">
											<xs:annotation>
												<xs:documentation source="Description" xml:lang="en">Passport and return airline ticket</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="GroundPromotionType">
		<xs:annotation>
			<xs:documentation source="Description" xml:lang="en">Promotions that apply to a ground transportation service.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="VendorCodes" type="xs:string" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation source="Description" xml:lang="en">A list of vendor codes associated with a promotion.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="PromotionCode" type="StringLength1to32" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">A promotion code used by the host to link directly with a specific named advertising campaign.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="GroundRateQualifierType">
		<xs:annotation>
			<xs:documentation source="Description" xml:lang="en">Rate qualifiers that apply to a ground transportation service.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Category" type="List_RateCategory" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="Description" xml:lang="en">An extensible list of rate categories.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TravelPurpose" type="List_TravelPurpose" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="Description" xml:lang="en">An extensible list of travel purpose types.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SpecialInputs" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation source="Description" xml:lang="en">Account specific additional information required to complete a booking (if the corporate account is being used). Up to five key value pair fields to be sent with the booking request if the requestor is using one of the corporate accounts requiring such information.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="Name" type="xs:string" use="required">
						<xs:annotation>
							<xs:documentation source="Description" xml:lang="en">The name of the special input.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="Value" type="xs:string" use="required">
						<xs:annotation>
							<xs:documentation source="Description" xml:lang="en">The value of the special input.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="AccountID" type="StringLength1to32" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">The customer account ID if applicable.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AccountName" type="StringLength1to128" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">The customer account name if applicable.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="RateQualifier" type="StringLength1to32" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">This is the vendor specific code for rate codes (e.g. WES, 2A, DLY00).</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CorpDiscountNmbr" type="StringLength1to32" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">The vendor specific code used to identify a special rate associated with a specific organization.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PromotionCode" type="StringLength1to32" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">Promotion code.</xs:documentation>
				<xs:documentation source="Example" xml:lang="en">Example: PROMO123</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="GroundReservationSummaryType">
		<xs:annotation>
			<xs:documentation source="Description" xml:lang="en">Summary information about one or more ground service reservations.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Confirmation" type="UniqueID_Type" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="Description" xml:lang="en">A confirmation number for the sucessfully booked ground transportation service reservation.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Contact" type="ContactPersonType" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="Description" xml:lang="en">Information about the primary contact associated with this reservation.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:choice minOccurs="0">
				<xs:element name="Service" minOccurs="0">
					<xs:annotation>
						<xs:documentation source="Description" xml:lang="en">Private service information.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element name="ServiceType" type="GroundServiceDetailType" minOccurs="0">
								<xs:annotation>
									<xs:documentation source="Description" xml:lang="en">The type of service.</xs:documentation>
								</xs:annotation>
							</xs:element>
							<xs:element name="Location" type="GroundLocationsType" minOccurs="0">
								<xs:annotation>
									<xs:documentation source="Description" xml:lang="en">Information about the locations associated with the reservation, including pickup, interim stop(s) and drop-off details.</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="Shuttle" minOccurs="0">
					<xs:annotation>
						<xs:documentation source="Description" xml:lang="en">Shuttle service information.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
