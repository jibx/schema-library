						<xs:extension base="RateQualifierType">
							<xs:attribute name="GolferRPH" type="RPH_Type" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="en">A reference to a unique golfer defined in Golfer/@RPH.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Golfer" type="GolferType" minOccurs="0" maxOccurs="99">
