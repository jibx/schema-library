<?xml version="1.0" encoding="UTF-8"?><!-- OPENTRAVEL SAMPLE INSTANCE --><!-- USE CASE SCENARIO: Request Air Availability for All Scheduled Flights from Origin/Destination with No Fare/Price Information. --><OTA_AirAvailRQ xmlns="http://www.opentravel.org/OTA/2003/05" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.opentravel.org/OTA/2003/05 OTA_AirAvailRQ.xsd" EchoToken="12345" TimeStamp="2010-12-17T09:30:47" Target="Production" SequenceNmbr="1" Version="4.001" PrimaryLangID="en-us">	<!-- Transaction originator (point of sale) information. -->	<POS>		<Source ISOCountry="US" ISOCurrency="USD">			<!-- Request type of 5 is "Travel Agency" - See OpenTravel Code List Unique ID Type (UIT). -->			<RequestorID Type="5" ID="12345"/>		</Source>	</POS>		<!-- A collection of information that specifies how the message processing should occur or how the data should be returned. -->	<!-- THIS IS WHERE YOU SPECIFY THE DISPLAY ORDER OF THE RESULTS -->	<ProcessingInfo DisplayOrder="ByArrivalTime"/>		<!-- Origin and Destination location, and time information for the Air Low Fare Search request. -->	<!-- AT LEAST ONE ORIGIN/DESTINATION PAIR IS REQUIRED -->	<!-- Leg 1 -->	<OriginDestinationInformation>		<DepartureDateTime>2011-01-05T10:00:00-05:00</DepartureDateTime>		<OriginLocation LocationCode="IAD"/>		<DestinationLocation LocationCode="FLG"/>	</OriginDestinationInformation>		<!-- Leg 2 - Return Trip -->	<OriginDestinationInformation>		<DepartureDateTime>2011-01-12T10:00:00-05:00</DepartureDateTime>		<OriginLocation LocationCode="FLG"/>		<DestinationLocation LocationCode="IAD"/>	</OriginDestinationInformation>	<!-- Air Availability Request preference information. -->	<TravelPreferences MaxStopsQuantity="1">		<FlightTypePref PreferLevel="Preferred" FlightType="Nonstop"/>		<EquipPref AirEquipType="757"/>		<CabinPref PreferLevel="Preferred" Cabin="Economy"/>		<TicketDistribPref PreferLevel="Only" DistribType="3"/>	</TravelPreferences>	<!-- Specify the number of passengers and types for Air Availability Search. -->	<!-- AT LEAST ONE AIRTRAVELERAVAIL IS REQUIRED -->	<TravelerInfoSummary>		<AirTravelerAvail>			<PassengerTypeQuantity Code="ADT" Quantity="2"/>		</AirTravelerAvail>	</TravelerInfoSummary></OTA_AirAvailRQ>