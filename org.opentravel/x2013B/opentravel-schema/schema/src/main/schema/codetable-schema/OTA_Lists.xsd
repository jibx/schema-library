<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:ns="http://www.opentravel.org/OTA/2003/05/common" elementFormDefault="qualified" attributeFormDefault="unqualified" version="0.000" id="OTA2013B">
	<xs:annotation>
		<xs:documentation source="Description" xml:lang="en">ALL SCHEMA FILES IN THE OPENTRAVEL ALLIANCE SPECIFICATION ARE MADE AVAILABLE ACCORDING TO THE TERMS DEFINED BY THE OPENTRAVEL LICENSE AGREEMENT AT http://www.opentravel.org/Specifications/Default.aspx.</xs:documentation>
	</xs:annotation>
	<xs:annotation>
		<xs:documentation source="Notice" xml:lang="en">The OTA_CodeType simpleType will be deprecated in the 2013B OpenTravel XML Message Suite publication. All OTA_CodeType typed attributes with be replaced with elements typed as an open list, e.g. List_NameOfList.</xs:documentation>
	</xs:annotation>
	<!--
		LIST EXTENSION MECHANISM
	-->
	<xs:attributeGroup name="OTA_CodeExtGroup">
		<xs:annotation>
			<xs:documentation source="Description" xml:lang="en">Code extension definition without dimension information.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="RemovalInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">Item removal indicator.</xs:documentation>
				<xs:documentation source="Example" xml:lang="en">Example: false</xs:documentation>
				<xs:documentation source="Implementer" xml:lang="en">Implementer: If true, this item is obsolete and should be removed from the receiving system.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Code" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">Proprietary code.</xs:documentation>
				<xs:documentation source="Example" xml:lang="en">Example: MYB</xs:documentation>
				<xs:documentation source="Implementer" xml:lang="en">Implementer: Place a code value in this attribute if you have selected the "Other_" value in the enumerated list. Note that this value should be known to your trading partners.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Description" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">Code description.</xs:documentation>
				<xs:documentation source="Example" xml:lang="en">Example: Party Supplies</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DescriptionDetail" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">Code description detail.</xs:documentation>
				<xs:documentation source="Example" xml:lang="en">Example: Mylar Baloons</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SourceName" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">Code authority or owner.</xs:documentation>
				<xs:documentation source="Example" xml:lang="en">Example: OpenTravel</xs:documentation>
				<xs:documentation source="Implementer" xml:lang="en">Implementer: This may be a source authority/ owner name, ID or code.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SourceURL" type="xs:anyURI" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">Code authority or owner URL.</xs:documentation>
				<xs:documentation source="Example" xml:lang="en">Example: http://www.opentravel.org</xs:documentation>
				<xs:documentation source="Implementer" xml:lang="en">Implementer: This is A URL to the code source authority/ owner.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ResourceName" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">Code resource name or ID.</xs:documentation>
				<xs:documentation source="Example" xml:lang="en">Example: OpenTravel 2012A CodeList</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ResourceURL" type="xs:anyURI" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">Code resource URL.</xs:documentation>
				<xs:documentation source="Example" xml:lang="en">Example: http://www.opentravel.org/somecodelist.html</xs:documentation>
				<xs:documentation source="Implementer" xml:lang="en">Implementer: This is A URL to the code resource.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="UniqueID" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">Unique ID for code.</xs:documentation>
				<xs:documentation source="Example" xml:lang="en">Example: MYB123</xs:documentation>
				<xs:documentation source="Implementer" xml:lang="en">Implementer: This is used for a database or key ID for the code item (if it is different from the @CodeExtension) in relationship to the obsolete code indicator.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:simpleType name="CodeListSchedulePeriodType">
		<xs:annotation>
			<xs:documentation source="Description" xml:lang="en">Operating schedule date, time or date/ time.</xs:documentation>
		</xs:annotation>
		<xs:union memberTypes="xs:date xs:dateTime xs:time"/>
	</xs:simpleType>
	<xs:complexType name="CodeListSummaryExtension">
		<xs:annotation>
			<xs:documentation source="Description" xml:lang="en">Simple Code List extension structure that supports OpenTravel open lists and third-party code lists.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="RemovalInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">Item removal indicator.</xs:documentation>
				<xs:documentation source="Example" xml:lang="en">Example: false</xs:documentation>
				<xs:documentation source="Implementer" xml:lang="en">Implementer: If true, this item is obsolete and should be removed from the receiving system.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Language" type="xs:language" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">Code information language.</xs:documentation>
				<xs:documentation source="Example" xml:lang="en">Example: en</xs:documentation>
				<xs:documentation source="MoreInfo" xml:lang="en">Note: This is the language that the code information is specified in.</xs:documentation>
				<xs:documentation source="MoreInfo" xml:lang="en">OpenTravel Best Practice for Language Designation: Unless otherwise noted, all language designations are two alpha characters and WILL follow ISO 639-1 prescribed format.</xs:documentation>
				<xs:documentation source="Reference" xml:lang="en">Reference: http://www.iso.org/iso/home.htm</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Code" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">Code value.</xs:documentation>
				<xs:documentation source="Example" xml:lang="en">Example: MYB</xs:documentation>
				<xs:documentation source="Implementer" xml:lang="en">Implementer: Place a code value in this attribute if you have selected the "Other_" value in the enumerated list. Note that this value should be known to your trading partners.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Description" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">Code description.</xs:documentation>
				<xs:documentation source="Example" xml:lang="en">Example: Party Supplies</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="DescriptionDetail" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">Code description detail.</xs:documentation>
				<xs:documentation source="Example" xml:lang="en">Example: Mylar Baloons</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SourceName" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">Code authority or owner.</xs:documentation>
				<xs:documentation source="Example" xml:lang="en">Example: OpenTravel</xs:documentation>
				<xs:documentation source="Implementer" xml:lang="en">Implementer: This may be a source authority/ owner name, ID or code.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SourceURL" type="xs:anyURI" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">Code authority or owner URL.</xs:documentation>
				<xs:documentation source="Example" xml:lang="en">Example: http://www.opentravel.org</xs:documentation>
				<xs:documentation source="Implementer" xml:lang="en">Implementer: This is A URL to the code source authority/ owner.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ResourceID" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">Code resource ID.</xs:documentation>
				<xs:documentation source="Example" xml:lang="en">Example: OTALIST</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ResourceName" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">Code resource name.</xs:documentation>
				<xs:documentation source="Example" xml:lang="en">Example: OpenTravel 2012A CodeList</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ResourceURL" type="xs:anyURI" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">Code resource URL.</xs:documentation>
				<xs:documentation source="Example" xml:lang="en">Example: http://www.opentravel.org/somecodelist.html</xs:documentation>
				<xs:documentation source="Implementer" xml:lang="en">Implementer: This is A URL to the code resource.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="UniqueID" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">Code unique ID.</xs:documentation>
				<xs:documentation source="Example" xml:lang="en">Example: MYB123</xs:documentation>
				<xs:documentation source="Implementer" xml:lang="en">Implementer: This is used for a database or key ID for the code item (if it is different from the @CodeExtension) in relationship to the obsolete code indicator.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="CodeListDetailExtension">
		<xs:annotation>
			<xs:documentation source="Description" xml:lang="en">Detail Code List extension structure that includes all Summary information plus quantity, proximity, charge, schedule and apply to level information.</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="CodeListSummaryExtension">
				<xs:sequence>
					<xs:element name="ApplyToLevel" type="List_ApplyToLevel" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation source="Description" xml:lang="en">Applicable item level.</xs:documentation>
							<xs:documentation source="Example" xml:lang="en">Example: Property</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Charge" type="CodeListFeeType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation source="Description" xml:lang="en">Associated charge information.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Schedule" type="CodeListScheduleType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation source="Description" xml:lang="en">Associated service schedule information.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:attribute name="Quantity" type="xs:positiveInteger" use="optional">
					<xs:annotation>
						<xs:documentation source="Description" xml:lang="en">Associated quantity for the item.</xs:documentation>
						<xs:documentation source="Example" xml:lang="en">Example: 2</xs:documentation>
					</xs:annotation>
				</xs:attribute>
				<xs:attribute name="Proximity" use="optional">
					<xs:annotation>
						<xs:documentation source="Description" xml:lang="en">Associated proximity for the item.</xs:documentation>
						<xs:documentation source="Example" xml:lang="en">Example: 2</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="Nearby"/>
							<xs:enumeration value="NotSpecified"/>
							<xs:enumeration value="Offsite"/>
							<xs:enumeration value="Onsite"/>
							<xs:enumeration value="OnsiteAndOffsite"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:attribute>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CodeListExtension">
		<xs:annotation>
			<xs:documentation source="Description" xml:lang="en">Choice between summary and detailed code extension information.</xs:documentation>
		</xs:annotation>
		<xs:choice>
			<xs:element name="Summary" type="CodeListSummaryExtension" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="Description" xml:lang="en">Summary code information.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Detail" type="CodeListDetailExtension" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="Description" xml:lang="en">Detail code information.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:choice>
		<xs:attribute name="ChargeInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">Charge information specified indicator.</xs:documentation>
				<xs:documentation source="Example" xml:lang="en">Example: true</xs:documentation>
				<xs:documentation source="Implementer" xml:lang="en">Implementer: If true, charge information has been specified and the Detail/Charge element should be parsed.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ProximityInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">Proximity information specified indicator.</xs:documentation>
				<xs:documentation source="Example" xml:lang="en">Example: true</xs:documentation>
				<xs:documentation source="Implementer" xml:lang="en">Implementer: If true, proximity information has been specified and the @Proximity attribute should be parsed.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="QuantityInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">Quantity information specified indicator.</xs:documentation>
				<xs:documentation source="Example" xml:lang="en">Example: true</xs:documentation>
				<xs:documentation source="Implementer" xml:lang="en">Implementer: If true, quantity information has been specified and the @Quantity attribute should be parsed.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ScheduleInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">Schedule information specified indicator.</xs:documentation>
				<xs:documentation source="Example" xml:lang="en">Example: true</xs:documentation>
				<xs:documentation source="Implementer" xml:lang="en">Implementer: If true, schedule information has been specified and the Detail/Schedule element should be parsed.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="CodeListFeeType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Fees associated with a code item.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Amount" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="Description" xml:lang="en">Fee amount or percentage.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:choice>
						<xs:element name="Currency">
							<xs:annotation>
								<xs:documentation source="Description" xml:lang="en">Money amount and currency encoding information.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:attribute name="Amount" type="xs:decimal" use="required">
									<xs:annotation>
										<xs:documentation source="Description" xml:lang="en">Fee or tax monetary amount.</xs:documentation>
										<xs:documentation source="Example" xml:lang="en">Example: 2500</xs:documentation>
										<xs:documentation source="MoreInfo" xml:lang="en">OpenTravel Best Practice for Money Amount Designation: Unless otherwise noted, all money amounts will contain the amount as a positive integer and currency designation. The decimal places (minor unit) should contain the number of decimal places associated with the amount. Example: 8500, USD, 2 represents $85.00USD.</xs:documentation>
									</xs:annotation>
								</xs:attribute>
								<xs:attribute name="CurrencyCode" use="optional">
									<xs:annotation>
										<xs:documentation source="Description" xml:lang="en">Currency code.</xs:documentation>
										<xs:documentation source="Example" xml:lang="en">Example: USD</xs:documentation>
										<xs:documentation source="MoreInfo" xml:lang="en">OpenTravel Best Practice for Currency Type Designation: Unless otherwise noted, all currency code designations are three alpha characters and WILL follow ISO 4217 prescribed format. Additionally, all currency amounts SHOULD have an ISO 4217 standard decimal places (minor units) specified.</xs:documentation>
										<xs:documentation source="Reference" xml:lang="en">Reference: http://www.iso.org/iso/home.htm</xs:documentation>
									</xs:annotation>
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:pattern value="[a-zA-Z]{3}"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:attribute>
								<xs:attribute name="DecimalPlaces" type="xs:nonNegativeInteger" use="optional">
									<xs:annotation>
										<xs:documentation source="Use" xml:lang="en">Currency code decimal places.</xs:documentation>
										<xs:documentation source="Developer Notes" xml:lang="en">The ISO 4217 standard "minor unit" is typically used when the amount provided includes the minor unit of currency without a decimal point (e.g., USD 8500 needs DecimalPlaces="2" to represent $85).</xs:documentation>
									</xs:annotation>
								</xs:attribute>
							</xs:complexType>
						</xs:element>
						<xs:element name="AlternateCurrency">
							<xs:annotation>
								<xs:documentation source="Description" xml:lang="en">Alternate currency information.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="List_LoyaltyPrgCurrency">
										<xs:attribute name="Quantity" type="xs:positiveInteger" use="required">
											<xs:annotation>
												<xs:documentation source="Description" xml:lang="en">Redemption unit quantity.</xs:documentation>
												<xs:documentation source="Example" xml:lang="en">Example: 50000</xs:documentation>
												<xs:documentation source="Moreinfo" xml:lang="en">Note: This is the quantity of the specified loyalty currency unit required for payment of this fee.</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="Percent">
							<xs:annotation>
								<xs:documentation source="Description" xml:lang="en">Fee percentage.</xs:documentation>
								<xs:documentation source="Example" xml:lang="en">Example: 6.25</xs:documentation>
								<xs:documentation source="MoreInfo" xml:lang="en">Note: This is a percentage of another amount that this fee or tax is based on.</xs:documentation>
							</xs:annotation>
							<xs:simpleType>
								<xs:restriction base="xs:decimal">
									<xs:minInclusive value="0.00"/>
									<xs:maxInclusive value="100.00"/>
								</xs:restriction>
							</xs:simpleType>
						</xs:element>
					</xs:choice>
				</xs:complexType>
			</xs:element>
			<xs:element name="Type" type="List_FeeTaxType" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="Description" xml:lang="en">Fee type.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Description" type="xs:string" minOccurs="0" maxOccurs="5">
				<xs:annotation>
					<xs:documentation source="Description" xml:lang="en">Fee description.</xs:documentation>
					<xs:documentation source="Implementer" xml:lang="en">Implementer: The associated language identifier for this descriptive information is specified in the @Language attribute.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Qualifiers" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="Description" xml:lang="en">Fee qualifier(s).</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ChargeUnit" minOccurs="0">
							<xs:annotation>
								<xs:documentation source="Description" xml:lang="en">Charge unit information.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Unit" minOccurs="0">
										<xs:annotation>
											<xs:documentation source="Description" xml:lang="en">Specific charge unit.</xs:documentation>
											<xs:documentation source="Example" xml:lang="en">Example: PerPerson</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:complexContent>
												<xs:extension base="List_ChargeUnit">
													<xs:attribute name="ExemptQty" type="xs:positiveInteger" use="optional">
														<xs:annotation>
															<xs:documentation source="Description" xml:lang="en">Exempt quantity.</xs:documentation>
															<xs:documentation source="Example" xml:lang="en">Example: 2</xs:documentation>
															<xs:documentation source="MoreInfo" xml:lang="en">Note: This is the number of units permitted before charges are applied.</xs:documentation>
														</xs:annotation>
													</xs:attribute>
													<xs:attribute name="MaximumQty" type="xs:positiveInteger" use="optional">
														<xs:annotation>
															<xs:documentation source="Description" xml:lang="en">Maximum units.</xs:documentation>
															<xs:documentation source="Example" xml:lang="en">Example: 7</xs:documentation>
															<xs:documentation source="MoreInfo" xml:lang="en">Note: This is the maximum number of charge units for which the charge will be applied.</xs:documentation>
														</xs:annotation>
													</xs:attribute>
												</xs:extension>
											</xs:complexContent>
										</xs:complexType>
									</xs:element>
									<xs:element name="Frequency" minOccurs="0">
										<xs:annotation>
											<xs:documentation source="Description" xml:lang="en">Charge frequency.</xs:documentation>
											<xs:documentation source="Example" xml:lang="en">Example: Daily</xs:documentation>
											<xs:documentation source="MoreInfo" xml:lang="en">Note: This is the timeframe used to apply the charge.</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:complexContent>
												<xs:extension base="List_ChargeFrequency">
													<xs:attribute name="ExemptQty" type="xs:positiveInteger" use="optional">
														<xs:annotation>
															<xs:documentation source="Description" xml:lang="en">Exempt quantity.</xs:documentation>
															<xs:documentation source="Example" xml:lang="en">Example: 2</xs:documentation>
															<xs:documentation source="MoreInfo" xml:lang="en">Note: This is the frequency exemption quantity before charges are applied.</xs:documentation>
														</xs:annotation>
													</xs:attribute>
													<xs:attribute name="MaximumQty" type="xs:positiveInteger" use="optional">
														<xs:annotation>
															<xs:documentation source="Description" xml:lang="en">Maximum quantity.</xs:documentation>
															<xs:documentation source="Example" xml:lang="en">Example: 7</xs:documentation>
															<xs:documentation source="MoreInfo" xml:lang="en">Note: This is the maximum number of times the charge will be applied (e.g. waive charges above 30 nights).</xs:documentation>
														</xs:annotation>
													</xs:attribute>
												</xs:extension>
											</xs:complexContent>
										</xs:complexType>
									</xs:element>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="EffectiveDateTime" type="xs:dateTime" use="optional">
						<xs:annotation>
							<xs:documentation source="Description" xml:lang="en">Fee starting date/ time.</xs:documentation>
							<xs:documentation source="Example" xml:lang="en">Example: 2013-01-13</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="ExpireDateTime" type="xs:dateTime" use="optional">
						<xs:annotation>
							<xs:documentation source="Description" xml:lang="en">Fee ending date/ time.</xs:documentation>
							<xs:documentation source="Example" xml:lang="en">Example: 2013-01-16</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="MaxAge" type="xs:positiveInteger" use="optional">
						<xs:annotation>
							<xs:documentation source="Description" xml:lang="en">Maximum age of applicability.</xs:documentation>
							<xs:documentation source="Example" xml:lang="en">Example: 62</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="MinAge" type="xs:positiveInteger" use="optional">
						<xs:annotation>
