<?xml version="1.0" encoding="UTF-8"?><!-- OPENTRAVEL SAMPLE INSTANCE --><!-- USE CASE SCENARIO: Request Lowest Fares for Each Class of Service with Fare Amount and Tax from Origin/Destination with Ordered Results by Fare Amount Low to High. --><OTA_AirLowFareSearchRQ xmlns="http://www.opentravel.org/OTA/2003/05" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.opentravel.org/OTA/2003/05 OTA_AirLowFareSearchRQ.xsd" EchoToken="12345" TimeStamp="2010-12-17T09:30:47" Target="Production" SequenceNmbr="1" Version="3.000" PrimaryLangID="en-us">	<!-- Transaction originator (point of sale) information. -->	<POS>		<Source ISOCountry="US" ISOCurrency="USD">			<!-- Request type of 5 is "Travel Agency" - See OpenTravel Code List Unique ID Type (UIT). -->			<RequestorID Type="5" ID="12345"/>		</Source>	</POS>		<!-- A collection of information that specifies how the message processing should occur or how the data should be returned. -->	<!-- THIS IS WHERE YOU SPECIFY THE DISPLAY ORDER OF THE RESULTS -->	<ProcessingInfo DisplayOrder="ByPriceLowToHigh"/>		<!-- Origin and Destination location, and time information for the Air Low Fare Search request. -->	<!-- AT LEAST ONE ORIGIN/DESTINATION PAIR IS REQUIRED -->	<!-- Leg 1 -->	<OriginDestinationInformation>		<DepartureDateTime>2011-01-05T10:00:00-05:00</DepartureDateTime>		<OriginLocation LocationCode="IAD"/>		<DestinationLocation LocationCode="FLG"/>	</OriginDestinationInformation>		<!-- Leg 2 - Return Trip -->	<OriginDestinationInformation>		<DepartureDateTime>2011-01-15T10:00:00-05:00</DepartureDateTime>		<OriginLocation LocationCode="FLG"/>		<DestinationLocation LocationCode="IAD"/>	</OriginDestinationInformation>		<!-- Specify actual airline, flight number, or booking class to qualify the low fare search. -->	<!-- THIS IS WHERE YOU SPECIFY THAT YOU WANT FARES FOR ALL CLASSES OF SERVICE. Note that in general, if no class of service is specified than all classes of service will be returned in results. -->	<!-- Check out Wikipedia for a common list of class codes used by airline suppliers: http://en.wikipedia.org/wiki/Travel_class and note that there are generally more than 1 class code per class of service. -->	<SpecificFlightInfo>		<BookingClassPref  ResBookDesigCode="F"/>			<!-- First class -->		<BookingClassPref ResBookDesigCode="C"/>			<!-- Business class -->		<BookingClassPref ResBookDesigCode="Y"/>			<!-- Economy class/ full fare -->	</SpecificFlightInfo>	<!-- Specify the number of passengers and types for Air Low Fare Search. -->	<!-- AT LEAST ONE AIRTRAVELERAVAIL IS REQUIRED -->	<TravelerInfoSummary>		<AirTravelerAvail>				<PassengerTypeQuantity Code="ADT" Quantity="1"/>					<!-- Code ADT = Adult. Use your own code or Refer to OpenTravel Code List Age Qualifying Code (AQC) -->		</AirTravelerAvail>	</TravelerInfoSummary>	</OTA_AirLowFareSearchRQ>