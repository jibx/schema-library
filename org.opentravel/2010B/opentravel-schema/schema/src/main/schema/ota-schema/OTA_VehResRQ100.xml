<?xml version="1.0" encoding="UTF-8"?><!-- OPENTRAVEL SAMPLE INSTANCE --><!-- USE CASE SCENARIO: Micro-Donation Information Collected in a Vehicle Reservation Request. --><!-- USE CASE DETAILS: 10.00 (€) donation is made from a French citizen using a JCB credit card with NO donation receipt requested. Because the donation is from France, no “StateCode” is required. --><OTA_VehResRQ xmlns="http://www.opentravel.org/OTA/2003/05" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.opentravel.org/OTA/2003/05OTA_VehResRQ.xsd" Version="2.000">	<POS>		<Source PseudoCityCode="WXYZ" ISOCountry="US">			<RequestorID Type="5" ID="88888888"/>		</Source>	</POS>	<VehResRQCore Status="All">		<VehRentalCore PickUpDateTime="2010-11-01T10:30:00" ReturnDateTime="2010-11-09T10:00:00">			<PickUpLocation LocationCode="AC6"/>			<ReturnLocation LocationCode="AC6"/>		</VehRentalCore>		<Customer>			<Primary>				<PersonName>					<NamePrefix>Mr.</NamePrefix>					<GivenName>Gilles</GivenName>					<Surname>Pansu</Surname>				</PersonName>				<Telephone PhoneTechType="1" AreaCityCode="33" PhoneNumber="493953330"/>				<Email EmailType="2">gilles.pansu@amadeus.com</Email>				<Address Type="2">					<StreetNmbr>12 rue du Général de Gaulle</StreetNmbr>					<CityName>Paris</CityName>					<PostalCode>75001</PostalCode>				</Address>			</Primary>		</Customer>		<VendorPref CompanyShortName="Hertz" Code="ZE" PreferLevel="Preferred"/>		<DriverType Age="29"/>	</VehResRQCore>		<!-- MICRO DONATION INFORMATION -->	<DonationInformation GDS_ID="WE" Language="FR" CountryCode="FR" AskForReceiptInd="false">		<!-- Required Front Office information  -->		<FrontOfficeInfo ProductVersion="MyProductVersion" ProductName="MyProductName" OfficeID="MyOfficeID" CorporateID="MyCorporateID"/>		<!-- Required credit card information  -->		<CreditCardInfo CardCode="JC" ExpireDate="0515" CardNumber="340022224444666" Currency="CAD" DonationAmount="10.00">			<!-- CardCode "JC" is JCB -->			<CardHolderName>Gilles Pansu</CardHolderName>		</CreditCardInfo>	</DonationInformation>	</OTA_VehResRQ>