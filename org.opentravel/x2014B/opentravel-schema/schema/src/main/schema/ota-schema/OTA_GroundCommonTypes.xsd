	<xs:complexType name="GroundShuttleType">
		<xs:annotation>
			<xs:documentation source="Description" xml:lang="en">Shuttle service information.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Vehicle" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="Description" xml:lang="en">Shuttle bus vehicle information, including vehicle type and capacity.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="Type" type="List_VehCategory" minOccurs="0">
							<xs:annotation>
								<xs:documentation source="Description" xml:lang="en">An extensible list of shuttle types and/or categories.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="VehicleSize" minOccurs="0">
							<xs:annotation>
								<xs:documentation source="Description" xml:lang="en">An extensible list of shuttle sizes.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:complexContent>
									<xs:extension base="List_VehSize">
										<xs:attribute name="MaxPassengerCapacity" type="xs:positiveInteger" use="optional">
											<xs:annotation>
												<xs:documentation source="Description" xml:lang="en">The maximum person capacity of the shuttle.</xs:documentation>
											</xs:annotation>
										</xs:attribute>
										<xs:attribute name="MaxBaggageCapacity" type="xs:positiveInteger" use="optional">
											<xs:annotation>
												<xs:documentation source="Description" xml:lang="en">The maximum per person baggage capacity of the shuttle.</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:extension>
								</xs:complexContent>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute name="DisabilityInd" type="xs:boolean" use="required">
						<xs:annotation>
							<xs:documentation source="Description" xml:lang="en">If true, this shuttle is disability compatible.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="ID" type="RPH_Type" use="optional">
						<xs:annotation>
							<xs:documentation source="Description" xml:lang="en">A unqiue ID for this shuttle that is set here and may be referenced elsewhere in the message.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="ServiceLocation" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation source="Description" xml:lang="en">A collection of service locations.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="GroundLocationType">
							<xs:sequence>
								<xs:element name="Schedule" type="OperationSchedulePlusChargeType" minOccurs="0" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation source="Description" xml:lang="en">Hours of operation and other schedule information.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="Operator" type="CompanyNameType" form="qualified" minOccurs="0" maxOccurs="1">
									<xs:annotation>
										<xs:documentation source="Description" xml:lang="en">The name of the shuttle operation company.</xs:documentation>
									</xs:annotation>
								</xs:element>
								<xs:element name="Contact" type="ContactPersonType" minOccurs="0" maxOccurs="unbounded">
									<xs:annotation>
										<xs:documentation source="Description" xml:lang="en">Contact information for the shuttle operation company.</xs:documentation>
									</xs:annotation>
								</xs:element>
							</xs:sequence>
							<xs:attribute name="Sequence" type="xs:positiveInteger" use="optional">
								<xs:annotation>
									<xs:documentation source="Description" xml:lang="en">A sequence number for the service location.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="PickupInd" type="xs:boolean" use="optional">
								<xs:annotation>
									<xs:documentation source="Description" xml:lang="en">If true, customers may board (be picked up) at this location.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="ShuttleRPH" type="RPH_Type" use="optional">
								<xs:annotation>
									<xs:documentation source="Description" xml:lang="en">A reference to a specific shuttle vehicle defined in Vehicle.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="TicketReqInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">If true, a ticket is required to board the shuttle.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ReservationReqInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">If true, a reservation is required to board the shuttle.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="GroundTotalChargeType">
		<xs:annotation>
			<xs:documentation source="Description" xml:lang="en">The estimated total charge for the services associated with this reservation. NOTE: This element typically contains the total of both ServiceCharges and Fees.</xs:documentation>
		</xs:annotation>
		<xs:attributeGroup ref="VehicleTotalChargeGroup"/>
	</xs:complexType>
</xs:schema>
