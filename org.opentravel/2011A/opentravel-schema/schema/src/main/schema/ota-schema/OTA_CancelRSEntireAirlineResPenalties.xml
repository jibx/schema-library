<?xml version="1.0" encoding="UTF-8"?><!-- OPENTRAVEL SAMPLE INSTANCE --><!-- USE CASE SCENARIO: Cancel an entire airline reservation. --><!-- OPENTRAVEL MESSAGE VERSION: 1.001 [2003B] --><!-- USE CASE DETAILS: A travel agent has sent a cancel message that includes the travel agent system PNR locator and the airline PNR locator. The travel agent receives this response confirming that the booking has been cancelled that includes information on cancellation penalties that were applied. --><OTA_CancelRS xmlns="http://www.opentravel.org/OTA/2003/05" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.opentravel.org/OTA/2003/05OTA_CancelRS.xsd" EchoToken="892345" TimeStamp="2010-03-17T09:30:47-05:00" Target="Production" Version="1.001" SequenceNmbr="1" Status="Cancelled">	<!-- Status Specifies the status for the transaction. -->	<!-- The presence of this element indicates the request message was processed successfully by the receiving system. -->	<Success/>	<!-- A warning is returned regarding the cancellation penalty charged. -->	<Warnings>		<Warning Type="11">Cancellation penalties apply.</Warning>			<!-- Warning type of 11 is "Advisory" - See OpenTravel Code List Error Warning Type (EWT). -->	</Warnings>	<!-- Contains rules associated with canceling a reservation as well as the supplier's cancellation number. -->	<CancelInfoRS>		<!-- A collection of cancellation rules. -->		<CancelRules>			<CancelRule CancelByDate="2010-03-16T05:00:00-05:00" Amount="50.00" CurrencyCode="USD">				<!-- Note that for no penalties to apply, the cancellation needed to be made by the prior day. -->			</CancelRule>		</CancelRules>		<!-- Contains the supplier's cancellation number. -->		<UniqueID Type="14" ID="X2A382">			<!-- Unique ID type of 14 is "Reservation" - See OpenTravel Code List Unique ID Type (UIT). -->			<CompanyName CompanyShortName="DL" TravelSector="1"/>				<!-- Travel Sector of 1 is "Air" - See OpenTravel Code List Travel Sector  (TVS). -->		</UniqueID>		</CancelInfoRS></OTA_CancelRS>