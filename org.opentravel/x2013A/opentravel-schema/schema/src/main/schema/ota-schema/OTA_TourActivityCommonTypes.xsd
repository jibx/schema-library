				<xs:documentation xml:lang="en">The general supplier website URL.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="OperatorInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">If true, this information pertains to the tour/ activity operator.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SupplierInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">If true, this information pertains to the tour/ activity supplier.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
