					<xs:complexContent>
						<xs:extension base="GolfAmenityType">
							<xs:attribute name="GolferRPH" type="RPH_Type" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="en">A reference to a unique golfer defined in Golfer/@RPH.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Policy" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">Describes the policies of the tee time round, such as  cancellation.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="General" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="en">A general policy description and optional policy type.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:simpleContent>
									<xs:extension base="xs:string">
										<xs:attribute name="Type" type="xs:string" use="optional">
											<xs:annotation>
												<xs:documentation xml:lang="en">The type of the policy, such as "Tee Time" or "Cart Rental".</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:extension>
								</xs:simpleContent>
							</xs:complexType>
						</xs:element>
						<xs:element name="CancelPolicy" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="en">Defines the tee time cancellation policy of the golf facility and accepted guarantee payment types.</xs:documentation>
							</xs:annotation>
							<xs:complexType>
								<xs:sequence>
									<xs:element name="Deadline" minOccurs="0">
										<xs:annotation>
											<xs:documentation xml:lang="en">Cancellation deadline, absolute or relative.</xs:documentation>
										</xs:annotation>
										<xs:complexType>
											<xs:attributeGroup ref="DeadlineGroup">
												<xs:annotation>
													<xs:documentation xml:lang="en">The absolute deadline or amount of offset time before a deadline for a payment of cancel goes into effect.</xs:documentation>
												</xs:annotation>
											</xs:attributeGroup>
										</xs:complexType>
									</xs:element>
									<xs:element name="AmountPercent" minOccurs="0">
