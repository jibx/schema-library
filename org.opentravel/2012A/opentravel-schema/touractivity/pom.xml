<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>

  <parent>
    <groupId>org.jibx.schema.org.opentravel._2012A</groupId>
    <artifactId>opentravel-schema-reactor</artifactId>
    <version>1.2.0-SNAPSHOT</version>
  </parent>
  
  <artifactId>org.jibx.schema.org.opentravel._2012A.touractivity</artifactId>
  <packaging>bundle</packaging>

  <name>Schema Library - org.opentravel.touractivity - 2012A - opentravel.org tour activity schema</name>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.felix</groupId>
                <artifactId>maven-bundle-plugin</artifactId>
                <configuration>
                    <instructions>	<!-- Export-Service is deprecated but required by due to a bug in bundle-plugin -->
                         <Export-Service>org.jibx.schema.org.opentravel._2012A.touractivity.ws.TourActivityService</Export-Service>
                    </instructions>
                </configuration>
            </plugin>
        </plugins>
    </build>

  <properties>
    <systemName>touractivity</systemName>
    <schemaFilter>OTA_TourActivity</schemaFilter>
    <include-binding>${basedir}/../base.touractivity/target/generated-sources/binding.xml</include-binding>
  </properties>

  <dependencies>
    <dependency>
      <groupId>org.jibx.schema.org.opentravel._2012A</groupId>
      <artifactId>org.jibx.schema.org.opentravel._2012A.base.touractivity</artifactId>
      <version>${project.version}</version>
    </dependency>
    <dependency>
      <groupId>org.jibx.schema.org.opentravel._2012A</groupId>
      <artifactId>org.jibx.schema.org.opentravel._2012A.misc</artifactId>
      <version>${project.version}</version>
    </dependency>
  </dependencies>

</project>
