																	<xs:extension base="TaxType"/>
																</xs:complexContent>
															</xs:complexType>
														</xs:element>
													</xs:sequence>
													<xs:attributeGroup ref="DateTimeSpanGroup">
														<xs:annotation>
															<xs:documentation xml:lang="en">Used to indicate the effective dates of the policy.</xs:documentation>
														</xs:annotation>
													</xs:attributeGroup>
													<xs:attribute name="Code" type="xs:string" use="optional">
														<xs:annotation>
															<xs:documentation xml:lang="en">The code identifying this policy.</xs:documentation>
														</xs:annotation>
													</xs:attribute>
													<xs:attribute name="LastUpdated" type="xs:dateTime" use="optional">
														<xs:annotation>
															<xs:documentation xml:lang="en">The date and time when the policy information for this facility was last updated.</xs:documentation>
														</xs:annotation>
													</xs:attribute>
													<xs:attributeGroup ref="DOW_PatternGroup">
														<xs:annotation>
															<xs:documentation xml:lang="en">Used to indicate the effective day for the policy.</xs:documentation>
														</xs:annotation>
													</xs:attributeGroup>
												</xs:complexType>
											</xs:element>
											<xs:element name="RateCategory" type="List_RateCategory" minOccurs="0">
												<xs:annotation>
													<xs:documentation source="Description" xml:lang="en">An extensible list of associated rate categories.</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element name="Rules" type="GolfAppliedRuleType" minOccurs="0" maxOccurs="unbounded">
												<xs:annotation>
													<xs:documentation source="Description" xml:lang="en">Rules that apply to the rate.</xs:documentation>
												</xs:annotation>
											</xs:element>
											<xs:element ref="TPA_Extensions" minOccurs="0"/>
										</xs:sequence>
										<xs:attribute name="Code">
											<xs:annotation>
												<xs:documentation source="Description" xml:lang="en">An ID or code for a golf course.</xs:documentation>
											</xs:annotation>
										</xs:attribute>
										<xs:attribute name="RateCode" type="StringLength1to64" use="optional">
											<xs:annotation>
												<xs:documentation source="Description" xml:lang="en">The rate ID or code.</xs:documentation>
											</xs:annotation>
										</xs:attribute>
										<xs:attribute name="RateName" type="xs:string" use="optional">
											<xs:annotation>
												<xs:documentation source="Description" xml:lang="en">The name of the rate.</xs:documentation>
											</xs:annotation>
										</xs:attribute>
										<xs:attribute name="RatePeriod" use="optional">
											<xs:annotation>
												<xs:documentation source="Description" xml:lang="en">The valid duration of the rate.</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="RatePeriodSimpleType"/>
											</xs:simpleType>
										</xs:attribute>
										<xs:attribute name="CorpDiscountNmbr" type="StringLength1to32" use="optional">
											<xs:annotation>
												<xs:documentation source="Description" xml:lang="en">A vendor specific code used to identify a special rate associated with a specific organization.</xs:documentation>
											</xs:annotation>
										</xs:attribute>
										<xs:attribute name="PromotionCode" type="StringLength1to32" use="optional">
											<xs:annotation>
												<xs:documentation source="Description" xml:lang="en">A promotion ID or code identifier assigned by the supplier.</xs:documentation>
											</xs:annotation>
										</xs:attribute>
										<xs:attribute name="CustomerCode" type="StringLength1to32" use="optional">
											<xs:annotation>
												<xs:documentation source="Description" xml:lang="en">An ID or code for a frequent player, member, etc.</xs:documentation>
											</xs:annotation>
										</xs:attribute>
									</xs:complexType>
								</xs:element>
							</xs:sequence>
							<xs:attribute name="Code">
								<xs:annotation>
									<xs:documentation source="Description" xml:lang="en">An code for a golf course.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="ID">
								<xs:annotation>
									<xs:documentation source="Description" xml:lang="en">An ID for a golf course.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="Name">
								<xs:annotation>
									<xs:documentation source="Description" xml:lang="en">A name for a golf course.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="ShortName">
								<xs:annotation>
									<xs:documentation source="Description" xml:lang="en">The short name of the golf course.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="PrepaidMethod" use="optional">
								<xs:annotation>
									<xs:documentation source="Description" xml:lang="en">Inclusion or exclusion of prepaid rates.</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="Exclude Prepaid">
											<xs:annotation>
												<xs:documentation xml:lang="en">Exclude prepaid rates from the response.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="Include Prepaid">
											<xs:annotation>
												<xs:documentation xml:lang="en">Return prepaid and non-prepaid rates.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
										<xs:enumeration value="Prepaid Only">
											<xs:annotation>
												<xs:documentation xml:lang="en">Only return prepaid rates.</xs:documentation>
											</xs:annotation>
										</xs:enumeration>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
							<xs:attribute name="AvailInd" type="xs:boolean" use="optional">
								<xs:annotation>
									<xs:documentation source="Description" xml:lang="en">If true, this tee time rate is available.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:complexType>
					</xs:element>
					<xs:element ref="TPA_Extensions" minOccurs="0"/>
				</xs:sequence>
				<xs:element name="Errors" type="ErrorsType" minOccurs="0">
					<xs:annotation>
						<xs:documentation xml:lang="en">Errors are returned if the request was unable to be processed.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:choice>
			<xs:attributeGroup ref="OTA_PayloadStdAttributes">
				<xs:annotation>
					<xs:documentation xml:lang="en">This element defines standard attributes that appear on the root element for all OpenTravel Messages.</xs:documentation>
				</xs:annotation>
			</xs:attributeGroup>
		</xs:complexType>
	</xs:element>
</xs:schema>
