<?xml version="1.0" encoding="UTF-8"?><!-- OPENTRAVEL SAMPLE INSTANCE --><!-- USE CASE SCENARIO: Request Availability for Specified Hotel Codes & Rate Plan. --><!-- USE CASE DETAILS: A merchant trading partner that is caching hotel rates information makes a request to a hotel trading partner for rates changes for four specified hotel codes that have Triple AAA rate plans. --><OTA_HotelInvChangeRQ xmlns="http://www.opentravel.org/OTA/2003/05" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.opentravel.org/OTA/2003/05 OTA_HotelInvChangeRQ.xsd" EchoToken="12345" TimeStamp="2010-07-17T09:30:47" Target="Production" Version="1.000" PrimaryLangID="en-us">	<!-- Transaction originator (point of sale) information -->	<POS>		<Source ISOCountry="US" ISOCurrency="USD">			<!-- Request type of 32 is "Merchant" - See OpenTravel Code List Unique ID Type (UIT). -->			<RequestorID Type="32" ID="ABCDEFGH" />		</Source>	</POS>	<!-- Request to check any changes in rates and availability since the specified time stamp (July 7, 2010 9:30 AM) -->	<CacheSearchCriteria CacheFromTimestamp="2010-07-17T09:30:47" AvailabilityInd="true" RatesInd="true">		<!-- Search criteria includes four hotel codes and a rate plan code for Triple AAA -->		<CacheSearchCriterion HotelCode="LONST" RatePlanCode="AAA" />		<CacheSearchCriterion HotelCode="LONSU" RatePlanCode="AAA"/>		<CacheSearchCriterion HotelCode="LONHB" RatePlanCode="AAA"/>		<CacheSearchCriterion HotelCode="LONZZ" RatePlanCode="AAA"/>	</CacheSearchCriteria></OTA_HotelInvChangeRQ>