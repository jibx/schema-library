<?xml version="1.0" encoding="UTF-8"?><!-- OPENTRAVEL SAMPLE INSTANCE --><!-- USE CASE SCENARIO: Request Availability for Specified Promotion Code. --><!-- USE CASE DETAILS: A travel agency trading partner that is caching hotel availability information makes a request to a hotel trading partner for availability changes for a specified promotion code (“TWOFREE”). --><OTA_HotelInvChangeRQ xmlns="http://www.opentravel.org/OTA/2003/05" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.opentravel.org/OTA/2003/05 OTA_HotelInvChangeRQ.xsd" EchoToken="12345" TimeStamp="2010-07-17T09:30:47" Target="Production" Version="1.000" PrimaryLangID="en-us">	<!-- Transaction originator (point of sale) information -->	<POS>		<Source ISOCountry="US" ISOCurrency="USD">			<!-- Request type of 5 is "Travel Agency" - See OpenTravel Code List Unique ID Type (UIT). -->			<RequestorID Type="5" ID="12345" />		</Source>	</POS>	<!-- Request to check any changes in rates and availability since the specified time stamp (July 7, 2010 9:30 AM) -->	<CacheSearchCriteria CacheFromTimestamp="2010-07-17T09:30:47" AvailabilityInd="true" RatesInd="true">	<!-- Search criteria includes the TWOFREE promotion code -->		<CacheSearchCriterion PromotionCode="TWOFREE"/>	</CacheSearchCriteria></OTA_HotelInvChangeRQ>