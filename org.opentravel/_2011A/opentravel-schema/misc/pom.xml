  <artifactId>org.jibx.schema.org.opentravel._2011A.misc</artifactId>
  <packaging>bundle</packaging>

  <name>Schema Library - org.opentravel.misc - 2011A - opentravel.org misc schema</name>

  <properties>
    <systemName>misc</systemName>
    <schemaFilter>*</schemaFilter>
  </properties>

  <build>
    <plugins>

      <plugin>
        <groupId>org.jibx</groupId>
        <artifactId>jibx-maven-plugin</artifactId>

        <executions>
          <execution>
            <id>generate-java-code-from-schema</id>
            <goals>
              <goal>schema-codegen</goal>
            </goals>
	        <configuration>
	        	<schemaLocation>${schemaLocation}</schemaLocation>
	            <customizations>
	            	<customization>${customization-file}</customization>
	            </customizations>
	            <includeSchemas>
	                <!-- include>*.xsd</include -->
	                <includeSchema>OTA_Cancel*.xsd</includeSchema>
	                <includeSchema>OTA_Authorization*.xsd</includeSchema>
	                <includeSchema>OTA_Cancel*.xsd</includeSchema>
	                <includeSchema>OTA_Delete*.xsd</includeSchema>
	                <includeSchema>OTA_Error*.xsd</includeSchema>
	                <includeSchema>OTA_FileAttachment*.xsd</includeSchema>
	                <includeSchema>OTA_Notif*.xsd</includeSchema>
	                <includeSchema>OTA_Purchase*.xsd</includeSchema>
	                <includeSchema>OTA_Read*.xsd</includeSchema>
	                <includeSchema>OTA_ResRetrieveRS.xsd</includeSchema>
	            </includeSchemas>
              <includeBaseBindings>
                <includeBaseBinding>
                  <includes>
                    <include>${basedir}/../base/target/generated-sources/binding.xml</include>
                <!-- 
                    <include>${basedir}/../hotel/target/generated-sources/binding.xml</include>
                    <include>${basedir}/../cruise/target/generated-sources/binding.xml</include>
                    <include>${basedir}/../pkg/target/generated-sources/binding.xml</include>
                    <include>${basedir}/../screen/target/generated-sources/binding.xml</include>
                    <include>${basedir}/../golf/target/generated-sources/binding.xml</include>
                    <include>${basedir}/../rail/target/generated-sources/binding.xml</include>
                    <include>${basedir}/../insurance/target/generated-sources/binding.xml</include>
                    <include>${basedir}/../dynamicpkg/target/generated-sources/binding.xml</include>
                    <include>${basedir}/../loyalty/target/generated-sources/binding.xml</include>
                    <include>${basedir}/../destactivity/target/generated-sources/binding.xml</include>
                    <include>${basedir}/../tour/target/generated-sources/binding.xml</include>
                    <include>${basedir}/../travelitinerary/target/generated-sources/binding.xml</include>
                    <include>${basedir}/../ping/target/generated-sources/binding.xml</include>
                 -->
                  </includes>
                </includeBaseBinding>
              </includeBaseBindings>
	        </configuration>
          </execution>
          <execution>
              <id>compile-binding</id>
              <goals>
                  <goal>bind</goal>
              </goals>
              <configuration>
                  <schemaBindingDirectory>target/generated-sources</schemaBindingDirectory>
                  <includeSchemaBindings>
                    <includeSchemaBinding>binding.xml</includeSchemaBinding>
                  </includeSchemaBindings>
              </configuration>
          </execution>
        </executions>
	  </plugin>

  </plugins>
  </build>
 
