<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:ns="http://www.opentravel.org/OTA/2003/05/common" elementFormDefault="qualified" attributeFormDefault="unqualified" version="4.000" id="OTA2012A">
	<xs:annotation>
		<xs:documentation source="Description" xml:lang="en">ALL SCHEMA FILES IN THE OPENTRAVEL ALLIANCE SPECIFICATION ARE MADE AVAILABLE ACCORDING TO THE TERMS DEFINED BY THE OPENTRAVEL LICENSE AGREEMENT AT http://www.opentravel.org/Specifications/Default.aspx.</xs:documentation>
	</xs:annotation>
	<xs:annotation>
    <xs:documentation source="Notice" xml:lang="en">The OTA_CodeType simpleType will be deprecated in the 2013A OpenTravel XML Message Suite publication. All OTA_CodeType typed attributes with be replaced with elements typed as an open list, e.g. List_NameOfList.</xs:documentation>
  </xs:annotation>
	<xs:include schemaLocation="OTA_CommonTypes.xsd"/>
	<xs:simpleType name="GolferTypeEnum">
		<xs:annotation>
			<xs:documentation>Golfer types with an "Other" value to support open enumeration list.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="ClubMember">
				<xs:annotation>
					<xs:documentation source="Description" xml:lang="en">TBC...</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="GuestOfMember">
				<xs:annotation>
					<xs:documentation source="Description" xml:lang="en">TBC...</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="GuestOfResort">
				<xs:annotation>
					<xs:documentation source="Description" xml:lang="en">TBC...</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LocalResident">
				<xs:annotation>
					<xs:documentation source="Description" xml:lang="en">TBC...</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LoyaltyProgramMember">
				<xs:annotation>
					<xs:documentation source="Description" xml:lang="en">TBC...</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Other_">
				<xs:annotation>
					<xs:documentation source="Description" xml:lang="en">This is an OpenTravel reserved word used to support an open enumeration that is known between trading partners. If you select this value, enter your golfer type into the @extension attribute.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:attributeGroup name="GolfTaxAmountGroup">
		<xs:annotation>
			<xs:documentation xml:lang="en">Defines information specific to one tax amount that is applicable to the cost of an item.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="Total" type="Money" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="en">The total amount of the tax.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="CurrencyCode" type="AlphaLength3" use="required">
			<xs:annotation>
				<xs:documentation xml:lang="en">The currency code that the total amount is represented in.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="TaxCode" type="StringLength1to16" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">A code for the tax that is agreed upon between trading partners.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Percentage" type="Percentage" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">A percentage of the defined charge amount that this tax represents.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Description" type="StringLength1to64" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">A description of the tax.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:attributeGroup>
	<xs:complexType name="GolfAppliedRuleType">
		<xs:annotation>
			<xs:documentation source="Description" xml:lang="en">Applied rule information, including category, rule identifier and rule descriptions.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:choice>
				<xs:annotation>
					<xs:documentation source="Use" xml:lang="en">A choice between a default rule indicator OR a rule name and version number.</xs:documentation>
				</xs:annotation>
				<xs:element name="DefaultUsedInd" type="xs:boolean">
					<xs:annotation>
						<xs:documentation source="Description" xml:lang="en">If true, a system default rule was used.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="RuleInfo">
					<xs:annotation>
						<xs:documentation source="Description" xml:lang="en">Information for individual tee time supplier applied rules.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:attribute name="Name" type="xs:string" use="required">
							<xs:annotation>
								<xs:documentation source="Description" xml:lang="en">The name of the applied rule.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
						<xs:attribute name="Version" type="xs:integer" use="optional">
							<xs:annotation>
								<xs:documentation source="Description" xml:lang="en">The version of the rule.</xs:documentation>
							</xs:annotation>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
			</xs:choice>
			<xs:element name="Description" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation source="Description" xml:lang="en">Rule description(s).</xs:documentation>
					<xs:documentation source="Developer" xml:lang="en">Repeated descriptions may be used to accommodate information categories.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="ShortDescription" type="FreeTextType" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="en">A short description of the rule text with a language identifier.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="LongDescription" type="FreeTextType" minOccurs="0">
							<xs:annotation>
								<xs:documentation xml:lang="en">A long description of the rule text with a language identifier.</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="Category" type="xs:string" use="required">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">The applied rule category.</xs:documentation>
				<xs:documentation source="Example" xml:lang="en">Advanced Purchase | Weekend Tee Time</xs:documentation>
			</xs:annotation>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="GolfAmenitySummaryType">
		<xs:annotation>
			<xs:documentation>Amentities and services requested and or available at the golf facility. Note, no pricing or policy information is included.</xs:documentation>
		</xs:annotation>
		<xs:attribute name="ID" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">A unique ID for this amenity.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AmenityCode" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>A code for the amenity.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Name" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">The name of the amenity.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Description" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>The amenity description.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PricingType" use="optional">
			<xs:annotation>
				<xs:documentation>How the amenity is priced, such as per person or per amenity.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="PerPerson"/>
					<xs:enumeration value="PerAmenity"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="IncludedInTeeTimePriceInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>If TRUE, this amenity is included with the tee time purchase.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ReservableInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>If TRUE, this amenity may be reserved.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SequentialMask">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">A string of numeric values that represent requested and/or available amenities.</xs:documentation>
				<xs:documentation source="Developer" xml:lang="en">(Developer Notes) An amenity mask may be used for systems that exchange amenity information in a sequential string format. For example, in a 20 character amenity string, if the fourth position represents free lunch and the tenth position represents a golf cart, the string would look like this:
00010000010000000000</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="[0-1]"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="GolfAmenityType">
		<xs:annotation>
			<xs:documentation>Amentities and services available at the golf course (either included in the price of the tee time or available for an extra fee), such as power carts. Note, includes pricing.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Fees" type="FeesType" minOccurs="0" maxOccurs="99">
				<xs:annotation>
					<xs:documentation xml:lang="en">The price associated with tee time amenities.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Policy" minOccurs="0" maxOccurs="9">
				<xs:annotation>
					<xs:documentation xml:lang="en">Summary amenity policies.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="Type" type="xs:string" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="en">The type of the policy, such as "Tee Time" or "Cart Rental".</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="PaymentGuarantee" type="PaymentFormType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Guarantee payment information (if required) for golf amenity reservation.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="ID" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">A unique ID for this amenity.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="AmenityCode" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>A code for the amenity.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Name" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation xml:lang="en">The name of the amenity.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Description" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>The amenity description.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PricingType" use="optional">
			<xs:annotation>
				<xs:documentation>How the amenity is priced, such as per person or per amenity.</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="PerPerson"/>
					<xs:enumeration value="PerAmenity"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
		<xs:attribute name="IncludedInTeeTimePriceInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>If TRUE, this amenity is included with the tee time purchase.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="ReservableInd" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>If TRUE, this amenity may be reserved.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="PolicyInfo" type="xs:string" use="optional">
			<xs:annotation>
				<xs:documentation>Optional policy information regarding the amenity, for example Power Cart rental policy.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="SequentialMask">
			<xs:annotation>
				<xs:documentation source="Description" xml:lang="en">A string of numeric values that represent requested and/or available amenities.</xs:documentation>
				<xs:documentation source="Developer" xml:lang="en">(Developer Notes) An amenity mask may be used for systems that exchange amenity information in a sequential string format. For example, in a 20 character amenity string, if the fourth position represents free lunch and the tenth position represents a golf cart, the string would look like this:
00010000010000000000</xs:documentation>
			</xs:annotation>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:pattern value="[0-1]"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:attribute>
	</xs:complexType>
	<xs:complexType name="GolfFacilityType">
		<xs:annotation>
			<xs:documentation xml:lang="en">Information about a golf facility, which may be a golf course proper and/or a facility such as a hotel that has an association with a nearby golf course or has one on premise.</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="UniqueID">
				<xs:annotation>
					<xs:documentation xml:lang="en">The unique ID and name of the golf facility.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="UniqueID_Type">
							<xs:attribute name="AssociatedFacility" type="StringLength1to128" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="en">The name of the facility that the golf course is associated with.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Amenities" type="GolfAmenitySummaryType" minOccurs="0" maxOccurs="99">
				<xs:annotation>
					<xs:documentation xml:lang="en">Amenities available at the golf facility. Note, these amenities typically apply to all tee times at the facility.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Contact" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Primary phone and website contact information for the golf facility.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attributeGroup ref="TelephoneGroup"/>
					<xs:attribute name="SiteURL" type="xs:anyURI" use="optional">
						<xs:annotation>
							<xs:documentation source="Description" xml:lang="en">The URL of the golf facility website.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="CourseClosure" type="DateTimeSpanType" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">A date and time period when a golf facility is closed.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CourseConditions" minOccurs="0" maxOccurs="99">
				<xs:annotation>
					<xs:documentation source="Description" xml:lang="en">Information about course conditions with an optional source name or URL.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="Source">
								<xs:annotation>
									<xs:documentation source="Description" xml:lang="en">The source name or URL that provided the course conditions.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="CourseRating" minOccurs="0" maxOccurs="99">
				<xs:annotation>
					<xs:documentation source="Description" xml:lang="en">Ratings for the course with an optional source name or URL.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="xs:string">
							<xs:attribute name="Source">
								<xs:annotation>
									<xs:documentation source="Description" xml:lang="en">The source name or URL that provided the course rating.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Directions" type="xs:string" minOccurs="0" maxOccurs="9">
				<xs:annotation>
					<xs:documentation xml:lang="en">Directions to the golf facility.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Features" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="Description" xml:lang="en">Facility features information.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="DressPolicy" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation source="Description" xml:lang="en">The dress code for the golf facility.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="DrivingRangeInd" type="xs:boolean" use="optional">
						<xs:annotation>
							<xs:documentation source="Description" xml:lang="en">If true, the golf facility contains a driving range.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="DiningInd" type="xs:boolean" use="optional">
						<xs:annotation>
							<xs:documentation source="Description" xml:lang="en">If true, the golf facility contains a dining facility.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="GolfLessonInd" type="xs:boolean" use="optional">
						<xs:annotation>
							<xs:documentation source="Description" xml:lang="en">If true, golf lessons with a golf pro are available.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="GolfSchoolInd" type="xs:boolean" use="optional">
						<xs:annotation>
							<xs:documentation source="Description" xml:lang="en">If true, a golf school is available.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="YearBuilt" type="YearOrYearMonthType" use="optional">
						<xs:annotation>
							<xs:documentation source="Description" xml:lang="en">The year the course was built.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="CourseDesigner" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation source="Description" xml:lang="en">The name of the course designer.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="MetalSpikesInd" type="xs:boolean" use="optional">
						<xs:annotation>
							<xs:documentation source="Description" xml:lang="en">If true, metal spikes are allowed at the golf facility.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="NumberOfHoles" type="xs:positiveInteger" use="optional">
						<xs:annotation>
							<xs:documentation source="Description" xml:lang="en">The number of holes on the golf facility.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="DisabilityInd" type="xs:boolean" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">If true, the golf facility accommodates physically challenged individuals.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="DisabilityServices" type="xs:string" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">A description of services available for disabled individuals.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="DayLockerInd" type="xs:boolean" use="optional">
						<xs:annotation>
							<xs:documentation source="Description" xml:lang="en">If true, the golf facility has lockers available to guests.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
					<xs:attribute name="ClubRentalInd" type="xs:boolean" use="optional">
						<xs:annotation>
							<xs:documentation source="Description" xml:lang="en">If true, golf clubs are available for rental at the golf facility.</xs:documentation>
						</xs:annotation>
					</xs:attribute>
				</xs:complexType>
			</xs:element>
			<xs:element name="CourseRestrictions" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation xml:lang="en">A date and time period when restrictions apply to golf facility tee times.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="DateTimeSpanType">
							<xs:attribute name="Type" type="xs:string" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="en">The type of facility and/or tee time restriction.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
							<xs:attribute name="Description" type="xs:string" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="en">A description of the facility and/or tee time restriction.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:complexContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Description" minOccurs="0">
				<xs:annotation>
					<xs:documentation source="Description" xml:lang="en">A short facility description with optional long description and club type.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:simpleContent>
						<xs:extension base="StringLength1to255">
							<xs:attribute name="ClubType" use="optional">
								<xs:annotation>
									<xs:documentation source="Description" xml:lang="en">The guest policy at the facility.</xs:documentation>
								</xs:annotation>
								<xs:simpleType>
									<xs:restriction base="xs:string">
										<xs:enumeration value="Open"/>
										<xs:enumeration value="PrivateMembersOnly"/>
										<xs:enumeration value="PrivateResidentsOnly"/>
										<xs:enumeration value="Reciprocal"/>
										<xs:enumeration value="SemiPrivate"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
							<xs:attribute name="LongDesc" type="xs:string" use="optional">
								<xs:annotation>
									<xs:documentation xml:lang="en">A long description of the golf facility.</xs:documentation>
								</xs:annotation>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="FeeStructure" minOccurs="0" maxOccurs="4">
				<xs:annotation>
					<xs:documentation source="Description" xml:lang="en">General green fee amounts.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:attribute name="FeeType" use="optional">
						<xs:annotation>
							<xs:documentation xml:lang="en">A descriptor for the fee amount, such as low, high and average.</xs:documentation>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:enumeration value="Low"/>
								<xs:enumeration value="Mid"/>
								<xs:enumeration value="High"/>
								<xs:enumeration value="Average"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:attribute>
					<xs:attributeGroup ref="CurrencyAmountGroup"/>
				</xs:complexType>
			</xs:element>
			<xs:element name="HoursOfOperation" type="OperationSchedulesType" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Hours of operation for the golf facility.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Location" minOccurs="0">
				<xs:annotation>
					<xs:documentation xml:lang="en">Address and geocoding information for the golf facility.</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:complexContent>
						<xs:extension base="AddressType">
							<xs:attributeGroup ref="PositionGroup"/>
						</xs:extension>
