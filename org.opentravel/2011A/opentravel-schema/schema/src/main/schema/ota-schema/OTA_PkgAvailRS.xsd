<?xml version="1.0" encoding="UTF-8"?><xs:schema xmlns="http://www.opentravel.org/OTA/2003/05" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.opentravel.org/OTA/2003/05" elementFormDefault="qualified" version="4.000" id="OTA2011A">	<xs:include schemaLocation="OTA_PkgCommonTypes.xsd"/>	<xs:annotation>		<xs:documentation xml:lang="en">All Schema files in the OpenTravel Alliance specification are made available according to the terms defined by the OpenTravel License Agreement at http://www.opentravel.org/Specifications/Default.aspx.</xs:documentation>	</xs:annotation>	<xs:element name="OTA_PkgAvailRS">		<xs:annotation>			<xs:documentation xml:lang="en">The message details the availability of a requested package</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:choice>				<xs:annotation>					<xs:documentation xml:lang="en">May have either errors or warnings - must have one if there is no success.</xs:documentation>				</xs:annotation>				<xs:sequence>					<xs:element name="Success" type="SuccessType">						<xs:annotation>							<xs:documentation xml:lang="en">The presence of this element indicates that the receiving system has successfully processed the preceding request message.</xs:documentation>						</xs:annotation>					</xs:element>					<xs:element name="Warnings" type="WarningsType" minOccurs="0">						<xs:annotation>							<xs:documentation xml:lang="en">The presence of this element indicates that, while the preceding request message has been successfully processed, the requeat may not have been fully satisfied.</xs:documentation>						</xs:annotation>					</xs:element>					<xs:element name="Package" minOccurs="0">						<xs:annotation>							<xs:documentation xml:lang="en">Details of the requested package.</xs:documentation>						</xs:annotation>						<xs:complexType>							<xs:sequence>								<xs:element name="URL" type="URL_Type" minOccurs="0">									<xs:annotation>										<xs:documentation xml:lang="en">A link to details of a package.</xs:documentation>									</xs:annotation>								</xs:element>								<xs:element name="CompanyName" type="CompanyNameType" minOccurs="0">									<xs:annotation>										<xs:documentation xml:lang="en">Identifies the company supplying the package.</xs:documentation>									</xs:annotation>								</xs:element>								<xs:element name="DateRange" minOccurs="0">									<xs:annotation>										<xs:documentation xml:lang="en">The Start date and Duration or End Date of the package.</xs:documentation>									</xs:annotation>									<xs:complexType>										<xs:attributeGroup ref="DateTimeSpanGroup"/>									</xs:complexType>								</xs:element>								<xs:element name="PriceInfo" minOccurs="0" maxOccurs="5">									<xs:annotation>										<xs:documentation xml:lang="en">Contains a guide price for the package and the occupancy level to which it applies. Can be used to supply both adult and child guide prices.</xs:documentation>									</xs:annotation>									<xs:complexType>										<xs:complexContent>											<xs:extension base="PkgPriceType">												<xs:attribute name="OtherPrice" type="Money" use="optional">													<xs:annotation>														<xs:documentation xml:lang="en">This attribute alllows for the inclusion of memorandum price information (e.g., the previous or brochure price for comparison when the guide price reflects a recent change).</xs:documentation>													</xs:annotation>												</xs:attribute>												<xs:attribute name="OtherPriceDescription" type="StringLength1to32" use="optional">													<xs:annotation>														<xs:documentation xml:lang="en">A description for the 'Other Price'.</xs:documentation>													</xs:annotation>												</xs:attribute>											</xs:extension>										</xs:complexContent>									</xs:complexType>								</xs:element>								<xs:element name="ItineraryItems" minOccurs="0">									<xs:annotation>										<xs:documentation xml:lang="en">A collection of elements to allow any item of inventory to be requested for a package. Normal usage is for the items to be presented in chronological sequence.</xs:documentation>									</xs:annotation>									<xs:complexType>										<xs:sequence>											<xs:element name="ItineraryItem" type="ItineraryItemResponseType" maxOccurs="99">												<xs:annotation>													<xs:documentation xml:lang="en">Details of the requested package.</xs:documentation>												</xs:annotation>											</xs:element>										</xs:sequence>									</xs:complexType>								</xs:element>								<xs:element name="Cautions" minOccurs="0">									<xs:annotation>										<xs:documentation xml:lang="en">A collection of items of essential information relating to components of a package itinerary.</xs:documentation>									</xs:annotation>									<xs:complexType>										<xs:sequence>											<xs:element name="Caution" type="PkgCautionType" maxOccurs="99">												<xs:annotation>													<xs:documentation xml:lang="en">Provides information regarding a package component which could affect a customer's decision to book.</xs:documentation>												</xs:annotation>											</xs:element>										</xs:sequence>									</xs:complexType>								</xs:element>							</xs:sequence>							<xs:attributeGroup ref="ID_OptionalGroup"/>							<xs:attribute name="Type" type="OTA_CodeType" use="optional">								<xs:annotation>									<xs:documentation xml:lang="en">Refer to OpenTravel Code List Package Type (PKG).</xs:documentation>								</xs:annotation>							</xs:attribute>							<xs:attribute name="TravelCode" type="PkgTravelCode" use="optional">								<xs:annotation>									<xs:documentation xml:lang="en">The supplier's code to identify the travel arrangement for a package.</xs:documentation>								</xs:annotation>							</xs:attribute>							<xs:attribute name="TourCode" type="StringLength1to8" use="optional">								<xs:annotation>									<xs:documentation xml:lang="en">The supplier's code to identify the accommodation/tour arrangement for a package.</xs:documentation>								</xs:annotation>							</xs:attribute>							<xs:attribute name="ShortDescription" type="ShortDescriptionType" use="optional">								<xs:annotation>									<xs:documentation xml:lang="en">A short description of the package tour.</xs:documentation>								</xs:annotation>							</xs:attribute>							<xs:attribute name="BoardCode" type="MealPlanCode" use="optional">								<xs:annotation>									<xs:documentation xml:lang="en">The code for a meal plan e.g. AB- as brochure, AI- all inclusive, BB- bed and breakfast, FB- full board, HB- Half Board, RO- room only,SC- self catering.</xs:documentation>								</xs:annotation>							</xs:attribute>							<xs:attribute name="PromotionCode" type="StringLength1to16" use="optional">								<xs:annotation>									<xs:documentation xml:lang="en">A code to request a specific promotional price.</xs:documentation>								</xs:annotation>							</xs:attribute>							<xs:attribute name="BrandCode" type="StringLength1to8" use="optional">								<xs:annotation>									<xs:documentation xml:lang="en">The code for the tour operator's product brand e.g. Thomascook, JMC, Sunset etc.</xs:documentation>								</xs:annotation>							</xs:attribute>							<xs:attribute name="ProductCode" type="StringLength1to8" use="optional">								<xs:annotation>									<xs:documentation xml:lang="en">The tour operator's code for the product within the brand e.g. Summersun, Greece etc.</xs:documentation>								</xs:annotation>							</xs:attribute>							<xs:attribute name="ExtrasInd" type="xs:boolean" use="optional">								<xs:annotation>									<xs:documentation xml:lang="en">When 'true', Extras are available with this package; when 'false', no Extras are available.</xs:documentation>								</xs:annotation>							</xs:attribute>						</xs:complexType>					</xs:element>					<xs:element name="TravelChoices" minOccurs="0">						<xs:annotation>							<xs:documentation xml:lang="en">A collection of TravelItem elements. Used by some operators when the requested travel item for the package is not available.</xs:documentation>						</xs:annotation>						<xs:complexType>							<xs:sequence>								<xs:element name="TravelItem" maxOccurs="9">									<xs:annotation>										<xs:documentation xml:lang="en">Details of a travel choice.</xs:documentation>									</xs:annotation>									<xs:complexType>										<xs:annotation>											<xs:documentation xml:lang="en">Details of a travel component e.g. a set of travel legs making up a travel component or a single item defining a round-trip journey.</xs:documentation>										</xs:annotation>										<xs:sequence>											<xs:choice>												<xs:element name="TravelDetail" minOccurs="0" maxOccurs="9">													<xs:annotation>														<xs:documentation xml:lang="en">Provides details of travel arrangements by segment.</xs:documentation>													</xs:annotation>													<xs:complexType>														<xs:sequence>															<xs:element name="OutwardTravel" type="PkgTravelSegment" maxOccurs="5"/>															<xs:element name="ReturnTravel" type="PkgTravelSegment" minOccurs="0" maxOccurs="5"/>														</xs:sequence>													</xs:complexType>												</xs:element>												<xs:element name="TravelJourney" minOccurs="0">													<xs:annotation>														<xs:documentation xml:lang="en">Provides information on a complete travel journey.</xs:documentation>													</xs:annotation>													<xs:complexType>														<xs:attributeGroup ref="PkgTravelJourneyGroup"/>													</xs:complexType>												</xs:element>											</xs:choice>											<xs:sequence>												<xs:element name="SupplementCharges" minOccurs="0">													<xs:annotation>														<xs:documentation xml:lang="en">A collection of supplementary charges.</xs:documentation>													</xs:annotation>													<xs:complexType>														<xs:sequence>															<xs:element name="SupplementCharge" type="FeeType" maxOccurs="9">																<xs:annotation>																	<xs:documentation xml:lang="en">A value representing a supplement for this journey (e.g., Aberdeen to Palma) over the cost of the journey on which package basic price is calculated (e.g., Gatwick to Palma).</xs:documentation>																</xs:annotation>															</xs:element>														</xs:sequence>													</xs:complexType>												</xs:element>												<xs:element name="Cautions" minOccurs="0">													<xs:annotation>														<xs:documentation xml:lang="en">A collection of items of essential information relating to travel items.</xs:documentation>													</xs:annotation>													<xs:complexType>														<xs:sequence>															<xs:element name="Caution" type="PkgCautionType" maxOccurs="9">																<xs:annotation>																	<xs:documentation xml:lang="en">Provides information regarding a package component which could affect a customer's decision to book.</xs:documentation>																</xs:annotation>															</xs:element>														</xs:sequence>													</xs:complexType>												</xs:element>											</xs:sequence>										</xs:sequence>										<xs:attribute name="AccomOKFlag" type="xs:boolean" use="optional">											<xs:annotation>												<xs:documentation xml:lang="en">Used to indicate whether or not the accommodation associated with the package is available with this travel journey.</xs:documentation>											</xs:annotation>										</xs:attribute>									</xs:complexType>								</xs:element>							</xs:sequence>						</xs:complexType>					</xs:element>					<xs:element name="AccommodationChoices" minOccurs="0">						<xs:annotation>							<xs:documentation xml:lang="en">A collection of available properties. Used by some operators when the requested accommodation item for the package is not available.</xs:documentation>						</xs:annotation>						<xs:complexType>							<xs:sequence>								<xs:element name="AvailableProperty" type="AccommodationDetailType" maxOccurs="99">									<xs:annotation>										<xs:documentation xml:lang="en">Details of a property available for the request date and duration</xs:documentation>									</xs:annotation>								</xs:element>							</xs:sequence>						</xs:complexType>					</xs:element>					<xs:element ref="TPA_Extensions" minOccurs="0"/>				</xs:sequence>				<xs:element name="Errors" type="ErrorsType">					<xs:annotation>						<xs:documentation xml:lang="en">The presence of this element indicates that the receiving system has been unable to process preceding request message.</xs:documentation>					</xs:annotation>				</xs:element>			</xs:choice>			<xs:attributeGroup ref="OTA_PayloadStdAttributes"/>			<xs:attributeGroup ref="CompanyID_AttributesGroup"/>			<xs:attributeGroup ref="CurrencyCodeGroup"/>		</xs:complexType>	</xs:element></xs:schema>